AAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA;AACA,QAAA,KAAA,CAAA,YAAA,GAAA,2CAAA;AACA,QAAA,KAAA,CAAA,QAAA,GAAA,IAAA;AACA,QAAA,KAAA,CAAA,UAAA,GAAA,KAAA;AACA,OAhBA;AAiBA,KA1BA;AA2BA,IAAA,UA3BA,sBA2BA,KA3BA,EA2BA,EA3BA,EA2BA;AAAA;;AACA,WAAA,QAAA,GAAA,IAAA;AACA,UAAA,MAAA,GAAA,EAAA;AACA,UAAA,KAAA,IAAA,CAAA,EAAA,MAAA,GAAA,CAAA,CAAA,KACA,MAAA,GAAA,CAAA;AACA,UAAA,OAAA,GAAA;AACA,QAAA,OAAA,EAAA,MADA;AAEA,QAAA,EAAA,EAAA;AAFA,OAAA;AAIA,MAAA,KAAA,CACA,GADA,CACA,iDADA,EACA;AACA,QAAA,MAAA,EAAA;AADA,OADA,EAIA,IAJA,CAIA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,YAAA,GAAA,kBAAA;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,IAAA;AACA,QAAA,MAAA,CAAA,UAAA,GAAA,OAAA;AACA,QAAA,MAAA,CAAA,aAAA,GAAA,KAAA;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,KAAA;AACA,OAVA,EAWA,KAXA,CAWA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA;AACA,QAAA,MAAA,CAAA,YAAA,GAAA,2CAAA;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,IAAA;AACA,QAAA,MAAA,CAAA,UAAA,GAAA,KAAA;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,KAAA;AACA,OAjBA;AAkBA;AAtDA,GAjEA;AAyHA,EAAA,OAzHA,qBAyHA;AAAA;;AACA,IAAA,KAAA,CACA,GADA,CACA,+CADA,EAEA,IAFA,CAEA,UAAA,QAAA,EAAA;AACA,MAAA,MAAA,CAAA,IAAA,GAAA,IAAA,CAAA,KAAA,CAAA,IAAA,CAAA,SAAA,CAAA,QAAA,CAAA,IAAA,CAAA,CAAA;AACA,MAAA,MAAA,CAAA,OAAA,GAAA,KAAA;AACA,KALA,EAMA,KANA,CAMA,UAAA,KAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,MAAA,MAAA,CAAA,YAAA,GAAA,2CAAA;AACA,MAAA,MAAA,CAAA,QAAA,GAAA,IAAA;AACA,MAAA,MAAA,CAAA,OAAA,GAAA,KAAA;AACA,KAXA;AAYA,GAtIA;AAuIA,EAAA,UAAA,EAAA;AACA,IAAA,MAAA,EAAA;AADA;AAvIA,CAAA","sourcesContent":["<template>\n  <v-container class=\"fill-height\">\n    <v-row>\n      <Breadcrumbs :page=\"page\" /><br />\n      <v-col cols=\"12\" xs=\"12\" sm=\"12\" md=\"12\">\n        <h2>\n          <p class=\"text-left\">\n            <v-icon x-large color=\"blue\">{{ icon }}</v-icon>\n            {{ $t(\"News.H1\") }}\n          </p>\n        </h2>\n        <v-card elevation=\"10\">\n          <v-card-title>\n            <v-text-field\n              v-model=\"search\"\n              label=\"Buscador\"\n              class=\"mx-4\"\n            ></v-text-field>\n          </v-card-title>\n          <v-card-text>\n            <v-btn\n              color=\"blue\"\n              fab\n              dark\n              small\n              absolute\n              top\n              right\n              @click=\"dialog = true\"\n            >\n              <v-icon>mdi-plus</v-icon>\n            </v-btn>\n            <v-data-table\n              :headers=\"headers\"\n              :items=\"news\"\n              :search=\"search\"\n              :loading=\"loading\"\n              :items-per-page=\"5\"\n              class=\"elevation-1\"\n            >\n              <template v-slot:item.Habilitado=\"{ item }\">\n                <v-simple-checkbox\n                  v-model=\"item.Habilitado\"\n                  @click=\"ChangeNews(item.Habilitado,item.Id)\"\n                >\n                  <template v-slot:label>\n                    <v-progress-circular\n                      v-show=\"switchMe\"\n                      :size=\"20\"\n                      indeterminate\n                      color=\"primary\"\n                    ></v-progress-circular>\n                  </template>\n                </v-simple-checkbox>\n              </template>\n            </v-data-table>\n          </v-card-text>\n        </v-card>\n        <v-dialog\n          v-model=\"dialog\"\n          transition=\"dialog-top-transition\"\n          max-width=\"600\"\n        >\n          <v-card elevation=\"10\">\n            <v-card-text>\n              <v-row>\n                <v-col cols=\"12\" xs=\"12\" sm=\"12\" md=\"12\">\n                  <v-form ref=\"form\" v-model=\"valid\" lazy-validation>\n                    <v-text-field\n                      v-model=\"title\"\n                      :rules=\"titleRules\"\n                      :label=\"$t('News.Title')\"\n                      prepend-icon=\"mdi-comment-outline\"\n                      counter\n                      maxlength=\"75\"\n                      required\n                    ></v-text-field>\n                    <br />\n                    <v-textarea\n                      v-model=\"descr\"\n                      :rules=\"descrRules\"\n                      counter\n                      maxlength=\"450\"\n                      :label=\"$t('News.Descr')\"\n                      prepend-icon=\"mdi-text-box-outline\"\n                      required\n                    >\n                    </v-textarea>\n                    <br />\n                    <v-text-field\n                      v-model=\"image\"\n                      :rules=\"imgRules\"\n                      counter\n                      maxlength=\"150\"\n                      prepend-icon=\"mdi-web\"\n                      :label=\"$t('News.Pick')\"\n                    ></v-text-field>\n                    <br />\n                    <v-menu\n                      ref=\"menu1\"\n                      v-model=\"menu1\"\n                      :close-on-content-click=\"false\"\n                      :return-value.sync=\"date1\"\n                      transition=\"scale-transition\"\n                      min-width=\"auto\"\n                    >\n                      <template v-slot:activator=\"{ on, attrs }\">\n                        <v-combobox\n                          v-model=\"date1\"\n                          :label=\"$t('News.Calendar')\"\n                          readonly\n                          v-bind=\"attrs\"\n                          v-on=\"on\"\n                          prepend-icon=\"mdi-calendar\"\n                        ></v-combobox>\n                      </template>\n\n                      <v-date-picker v-model=\"date1\" no-title scrollable>\n                        <v-spacer></v-spacer>\n                        <v-btn text color=\"primary\" @click=\"menu1 = false\">\n                          Cancel\n                        </v-btn>\n                        <v-btn\n                          text\n                          color=\"primary\"\n                          @click=\"$refs.menu1.save(date1)\"\n                        >\n                          OK\n                        </v-btn>\n                      </v-date-picker>\n                    </v-menu>\n\n                    <v-btn\n                      :disabled=\"!valid\"\n                      color=\"success\"\n                      class=\"mr-4\"\n                      @click=\"validate\"\n                    >\n                      Guardar\n                    </v-btn>\n                  </v-form>\n                </v-col>\n              </v-row>\n            </v-card-text>\n          </v-card>\n        </v-dialog>\n      </v-col>\n    </v-row>\n    <v-snackbar v-model=\"snackbar\" color=\"green\">\n      {{ snackbarText }}\n      <template v-slot:action=\"{ attrs }\">\n        <v-btn color=\"white\" text v-bind=\"attrs\" @click=\"snackbar = false\">\n          Close\n        </v-btn>\n      </template>\n    </v-snackbar>\n  </v-container>\n</template>\n<script>\nimport Breadcrumbs from \"../../components/Breadcrumbs\";\nimport { Ripple } from \"vuetify/lib/directives\";\nimport axios from \"axios\";\nexport default {\n  components: {\n    Breadcrumbs,\n  },\n  data() {\n    const Storeduser = JSON.parse(localStorage.getItem(\"USER_AUTH\"));\n    return {\n      valid: true,\n      dialog: false,\n      page: this.$route.meta.breadcrum[0].name,\n      icon: this.$route.meta.breadcrum[0].icon,\n      User: Storeduser,\n      loading: true,\n      search: \"\",\n      switchMe: false,\n      menu1: false,\n      date1: [],\n      snackbar: false,\n      snackbarText: \"\",\n      snackbarColor: \"Red\",\n      picker: new Date().toISOString().substr(0, 10),\n      title: \"\",\n      titleRules: [\n        (v) => !!v || \"Title is required\",\n        (v) => (v && v.length > 10) || \"Title must be more than 10 characters\",\n      ],\n      descr: \"\",\n      descrRules: [\n        (v) => !!v || \"Description is required\",\n        (v) =>\n          (v && v.length > 10) || \"Description must be more than 25 characters\",\n      ],\n      image: null,\n      imgRules: [\n        (v) => !!v || \"Url is required\",\n        (v) => (v && v.length > 10) || \"Url must be more than 25 characters\",\n      ],\n      headers: [\n        {\n          text: \"(#)ID\",\n          align: \"start\",\n          sortable: false,\n          value: \"Id\",\n          class: \"Headertbldh\",\n        },\n        { text: \"Titulo\", value: \"Titulo\", class: \"Headertbldh\" },\n        { text: \"Descripcion\", value: \"Cuerpo\", class: \"Headertbldh\" },\n        {\n          text: \"Fecha de publicacion\",\n          value: \"Fecha_publicacion\",\n          class: \"Headertbldh\",\n        },\n        { text: \"Estatus\", value: \"Habilitado\", class: \"Headertbldh\" },\n      ],\n      news: [\n        {\n          Id: \"\",\n          Titulo: \"\",\n          Cuerpo: \"\",\n          Fecha_publicacion: \"\",\n          Habilitado: \"\",\n        },\n      ],\n    };\n  },\n  methods: {\n    validate() {\n      let postdata = {\n        Titulo: this.title,\n        Cuerpo: this.descr,\n        Imagen: this.image,\n        Fecha: this.date1,\n        Emplid: this.User.emplid,\n      };\n      axios\n        .get(\"http://192.168.1.238:92/api/Noticias/alta.php\", {\n          params: postdata,\n        })\n        .then((response) => {\n          console.log(response.data);\n          this.items = JSON.parse(JSON.stringify(response.data));\n          this.snackbarText = \"Noticia creada\";\n          this.snackbar = true;\n          this.snackcolor = \"green\";\n        })\n        .catch((error) => {\n          console.log(response.data);\n          this.snackbarText = \"Ocurrió un error al cargar la informacion\";\n          this.snackbar = true;\n          this.snackcolor = \"red\";\n        });\n    },\n    ChangeNews(event, id) {\n      this.switchMe = true;\n      let status = \"\";\n      if (event == 0) status = 0;\n      else status = 1;\n      let postada = {\n        estatus: status,\n        id: id,\n      };\n      axios\n        .get(\"http://192.168.1.238:92/api/Noticias/update.php\", {\n          params: postada,\n        })\n        .then((response) => {\n          this.snackbarText = \"Cambio realizado\";\n          this.snackbar = true;\n          this.snackcolor = \"green\";\n          this.dialogvalores = false;\n          this.switchMe = false;\n        })\n        .catch((error) => {\n          console.log(response.data);\n          this.snackbarText = \"Ocurrió un error al cargar la informacion\";\n          this.snackbar = true;\n          this.snackcolor = \"red\";\n          this.switchMe = false;\n        });\n    },\n  },\n  mounted() {\n    axios\n      .get(\"http://192.168.1.238:92/api/Noticias/show.php\")\n      .then((response) => {\n        this.news = JSON.parse(JSON.stringify(response.data));\n        this.loading = false;\n      })\n      .catch((error) => {\n        console.log(error);\n        this.snackbarText = \"Ocurrió un error al cargar la informacion\";\n        this.snackbar = true;\n        this.loading = false;\n      });\n  },\n  directives: {\n    Ripple,\n  },\n};\n</script>\n"],"sourceRoot":"src/views/Administracion"}]}