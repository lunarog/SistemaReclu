            </v-date-picker>\r\n            </v-menu>\r\n\r\n            <v-select\r\n              v-model=\"general\"\r\n              :items=\"generalItems\"\r\n              item-value=\"value\"\r\n              item-text=\"text\"\r\n              :label=\"$t('UserInfo.Field5')\"\r\n              required\r\n            ></v-select>\r\n\r\n            <v-select\r\n              v-model=\"sub1\"\r\n              :items=\"sub1Items\"\r\n              item-value=\"value\"\r\n              item-text=\"text\"\r\n              :label=\"$t('UserInfo.Field6')\"\r\n              @change=\"onChange()\"\r\n              required\r\n            ></v-select>\r\n\r\n            <v-select\r\n              v-model=\"sub2\"\r\n              :items=\"sub2Items\"\r\n              item-value=\"value\"\r\n              item-text=\"text\"\r\n              :label=\"$t('UserInfo.Field7')\"\r\n              required\r\n            ></v-select>\r\n\r\n            <v-select\r\n              v-model=\"estatus\"\r\n              :items=\"estatusItems\"\r\n              item-value=\"value\"\r\n              item-text=\"text\"\r\n              :label=\"$t('UserInfo.Field8')\"\r\n              data-vv-name=\"selectProfile\"\r\n              required\r\n            ></v-select>\r\n\r\n            <v-text-field\r\n              v-model=\"coments\"\r\n              :rules=\"comentsRules\"\r\n              :label=\"$t('UserInfo.Field9')\"\r\n              required\r\n            ></v-text-field>\r\n            <v-btn\r\n              :disabled=\"!valid\"\r\n              color=\"success\"\r\n              class=\"mr-4\"\r\n              @click=\"Save()\"\r\n            >\r\n              {{ $t(\"UserInfo.Field10\") }}\r\n            </v-btn>\r\n          </v-form>\r\n        </v-card-text>\r\n        <div style=\"flex: 1 1 auto;\"></div>\r\n      </v-card>\r\n    </v-dialog>\r\n    <v-snackbar v-model=\"snackbar\" :color=\"snackcolor\">\r\n      {{ snackbarText }}\r\n      <template v-slot:action=