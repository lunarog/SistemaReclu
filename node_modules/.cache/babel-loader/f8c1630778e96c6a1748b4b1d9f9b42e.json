UA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,YAAA;AACA;AACA;AA1BA,CAAA","sourcesContent":["<template>\n  <v-app>\n    <Verticalmenu v-if=\"usuario != null\" />\n    <div v-if=\"usuario != null\" class=\"main\">\n      <router-view></router-view>\n    </div>\n    <div v-else class=\"login\">\n      <router-view></router-view>\n    </div>\n    <Footer v-if=\"usuario != null\" />\n  </v-app>\n</template>\n\n<script>\nimport Verticalmenu from \"./components/Verticalmenu\";\nimport Footer from \"./components/Footer\";\nimport { mapActions, mapState } from \"vuex\";\nimport axios from \"axios\";\n\nexport default {\n  name: \"App\",\n  components: {\n    Verticalmenu,\n    Footer,\n    sessionWatcher: null,\n  },\n  data: () => ({}),\n  computed: {\n    ...mapState([\"usuario\"]),\n    ...mapActions([\"updatemenu\"]),\n  },\n  created() {\n    if (localStorage.getItem(\"USER_AUTH\") !== null) {\n      const Storeduser = JSON.parse(localStorage.getItem(\"USER_AUTH\"));\n      let perfil = Storeduser.rol;\n      axios\n        .get(\"http://192.168.1.238:92/api/UpdateMenu.php?rol=\" + perfil)\n        .then((result) => {\n          let data = result.data.split(\"||\");\n          let Menu = data[0];\n          localStorage.setItem(\"MENU\", Menu);\n        });\n    } else {\n      console.log(\"Sin sesion\");\n    }\n  },\n};\n</script>\n<style scoped>\n@import \"styles/global.css\";\n</style>\n"],"sourceRoot":"src"}]}