  axios({\n        method: 'post',\n        url: 'http://www.ti.com/wsSeguridadUHP/Service.asmx?wsdl',\n        data: {\n          username:\"rogelioluna\",\n          pwd:\"Llaves20201\",\n          dominio:\"UHP\"\n        }\n      })\n      .then(function (response) {\n        console.log(response)\n        if(response){\n          let usuarioIngreso = {\n            username : \"roger\",\n            rol : \"admin\",\n            emplid : \"13086\",\n            nombre: \"Rogelio Luna Delgado\",\n            puesto: \"Programador web\",\n            departamento : \"Deserrollo humano\",\n            area : \"Desarrollo Humano\",\n            areaid: \"1\",\n            deptid: \"111-106\",\n            mail: \"rogelioluna@urrea.net\",\n            resp: \"Tsipin Irieta Amezcua Reyez\",\n            respemplid: \"8631\",\n            respmail: \"tirieta@urrea.net\",\n            menu:[\n              { title: 'Home',  rute: \"/\",        icon: 'mdi-view-dashboard' },\n              { title: 'About', rute: \"/About\",   icon: 'mdi-forum' },\n              { title: 'Login', rute: \"/Login\",   icon: 'mdi-forum' },\n            ]\n          }\n          localStorage.setItem('USER_AUTH',JSON.stringify(usuarioIngreso));\n          console.log()\n          commit('setUsuario',usuarioIngreso);\n          router.push(\"/\");\n        }\n      })\n      .catch(function (error) {\n        console.log(error);\n      });\n    },\n    checkAuth({commit}){\n      let userauth = JSON.parse(localStorage.getItem(\"USER_AUTH\"));\n      if(userauth == null){\n        router.push(\"/login\");\n        commit('setUsuario',null);\n      }else{\n        commit('setUsuario',userauth);\n      }\n    },\n    logOut ({commit}){\n      commit('setUsuario',null);\n      storage.clear();\n      router.push(\"/login\",userauth);\n    }\n  },\n  modules: {\n  }\n})\n"]}]}