<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<Sitio>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>*/\r\n  {\r\n    path: '/',\r\n    name: 'Home',\r\n    component: Home,\r\n    beforeEnter: ifAuthenticated\r\n  },\r\n  {\r\n    path: '/login',\r\n    name: 'Login',\r\n    component: () => import('../views/Login.vue'),\r\n    beforeEnter: ifNotAuthenticated\r\n\r\n  },  \r\n  {\r\n    path: '/Settings',\r\n    name: 'Settings',\r\n    component: () => import('../views/Settings.vue'),\r\n    beforeEnter: ifAuthenticated\r\n  },\r\n  /* <<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<Mi empresa>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>*/\r\n  {\r\n    path: '/Perfil',\r\n    name: 'perfil',\r\n    meta:{\r\n        breadcrum:[\r\n            {name:'Perfil',icon:'mdi-account'}\r\n        ]\r\n    },\r\n    component: () => import('../views/Mi_persona/Profile.vue'),\r\n    beforeEnter: ifAuthenticated\r\n  },\r\n  {\r\n    path: '/Calendar',\r\n    name: 'calendar',\r\n    meta:{\r\n        breadcrum:[\r\n            {name:'Calendario',icon:'mdi-account'}\r\n        ]\r\n    },\r\n    component: () => import('../views/Mi_persona/Calendar.vue'),\r\n    beforeEnter: ifAuthenticated\r\n\r\n  },\r\n  /* <<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<Mi empresa>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>*/\r\n  {\r\n    path: '/360_plantas',\r\n    name: '360_plantas',\r\n    meta:{\r\n        breadcrum:[\r\n            {name:'360 para plantas'}\r\n        ]\r\n    },\r\n    component: () => import('../views/Mi_empresa/360_plantas.vue'),\r\n    beforeEnter: ifAuthenticated\r\n\r\n  },\r\n  {\r\n    path: '/registro_visitantes',\r\n    name: 'registro_visitantes',\r\n    meta:{\r\n        breadcrum:[\r\n            {name:'registro de visitantes'}\r\n        ]\r\n    },\r\n    component: () => import('../views/Mi_empresa/registro_visitantes.vue'),\r\n    beforeEnter: ifAuthenticated\r\n\r\n  },\r\n  /* <<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<Mi equipo>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>*/\r\n  {\r\n    path: '/administracion_puestos',\r\n    name: 'administracion_puestos',\r\n    meta:{\r\n        breadcrum:[\r\n            {name:'administracion puestos'}\r\n        ]\r\n    },\r\n    component: () => import('../views/Mi_equipo/administracion_puestos.vue'),\r\n    beforeEnter: ifAuthenticated\r\n\r\n  },\r\n  /* <<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<Administracion>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>*/\r\n  {\r\n    path: '/Nuevousr',\r\n    name: 'Nuevousr',\r\n    meta:{\r\n        breadcrum:[\r\n            {name:'Alta de usuario',icon:'mdi-view-dashboard'}\r\n        ]\r\n    },\r\n    component: () => import('../views/Administracion/Alta_usuario.vue'),\r\n    beforeEnter: ifAuthenticated\r\n\r\n  },\r\n  {\r\n    path: '/Activos',\r\n    name: 'Activos',\r\n    meta:{\r\n        breadcrum:[\r\n            {name:'Empleados activos',icon:'mdi-view-dashboard'}\r\n        ]\r\n    },\r\n    component: () => import('../views/Administracion/Employees.vue'),\r\n    beforeEnter: ifAuthenticated\r\n\r\n  },\r\n  {\r\n    path: '/Userinfo/:emplid,:rol',\r\n    name: 'Userinfo',\r\n    meta:{\r\n        breadcrum:[\r\n            {name:'Informacion de empleado',icon:'mdi-view-dashboard'}\r\n        ]\r\n    },\r\n    component: () => import('../views/Administracion/Profile_adm.vue'),\r\n    beforeEnter: ifAuthenticated\r\n\r\n  },\r\n  {\r\n    path: '/News/',\r\n    name: 'News',\r\n    meta:{\r\n        breadcrum:[\r\n            {name:'Alta de noticias',icon:'mdi-view-dashboard'}\r\n        ]\r\n    },\r\n    component: () => import('../views/Administracion/News.vue'),\r\n    beforeEnter: ifAuthenticated\r\n\r\n  },\r\n  {\r\n    path: '/360_plantas_adm',\r\n    name: '360_plantas_adm',\r\n    meta:{\r\n        breadcrum:[\r\n            {name:'Administracion de plantas'}\r\n        ]\r\n    },\r\n    component: () => import('../views/Administracion/360_plantas_adm.vue'),\r\n    beforeEnter: ifAuthenticated\r\n\r\n  },\r\n  {\r\n    path: '/Ofertas_economicas',\r\n    name: 'ofertas_economicas',\r\n    meta:{\r\n        breadcrum:[\r\n            {name:'Oferta economica'}\r\n        ]\r\n    },\r\n    component: () => import('../views/Administracion/Ofertas_economicas.vue'),\r\n    beforeEnter: ifAuthenticated\r\n\r\n  },\r\n  {\r\n    path: '/Ofertas_economicas_adm',\r\n    name: 'Ofertas_economicas_adm',\r\n    meta:{\r\n        breadcrum:[\r\n            {name:'Administracion de ofertas economicas',icon:'mdi-view-dashboard'}\r\n        ]\r\n    },\r\n    component: () => import('../views/Administracion/Ofertas_economicas_adm.vue'),\r\n    beforeEnter: ifAuthenticated\r\n\r\n  }\r\n]\r\nconst router = new VueRouter({\r\n  mode: 'history',\r\n  base: process.env.BASE_URL,\r\n  routes\r\n})\r\n\r\nexport default router\r\n"]}]}