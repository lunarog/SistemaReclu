,OAAA,GAAA,KAAA;AACA,KAVA;AAWA;AA1GA,CAAA","sourcesContent":["<template>\r\n  <v-container class=\"fill-height\">\r\n    <v-row>\r\n      <v-col cols=\"12\" xs=\"11\" sm=\"11\" md=\"8\">\r\n        <div class=\"d-block d-sm-none\">\r\n          <v-img src=\"@/assets/logo.png\"></v-img>\r\n        </div>\r\n      </v-col>\r\n    </v-row>\r\n    <v-row align=\"center\" justify=\"center\">\r\n      <v-col cols=\"12\" xs=\"11\" sm=\"11\" md=\"8\">\r\n        <v-card elevation=\"10\" height=\"300\">\r\n          <v-card-title>\r\n            {{ $t(\"Home.H1\") }}\r\n          </v-card-title>\r\n          <v-card-text>\r\n            <v-carousel height=\"210\" cycle show-arrows-on-hover>\r\n              <v-carousel-item v-for=\"item in news\" :key=\"item.id\">\r\n                <v-sheet\r\n                  style=\"background-color: #fff;border-color: #fff;color: #000;\"\r\n                >\r\n                  <v-row>\r\n                    <v-col cols=\"6\" xs=\"6\" sm=\"4\" md=\"4\">\r\n                      <v-img :src=\"item.Url\"></v-img>\r\n                    </v-col>\r\n                    <v-col cols=\"6\" xs=\"6\" sm=\"8\" md=\"8\">\r\n                      <h2>{{ item.Titulo }}</h2>\r\n                      <h4>{{ item.Cuerpo }}</h4>\r\n                    </v-col>\r\n                  </v-row>\r\n                </v-sheet>\r\n              </v-carousel-item>\r\n            </v-carousel>\r\n          </v-card-text>\r\n        </v-card>\r\n      </v-col>\r\n      <v-col cols=\"12\" xs=\"11\" sm=\"11\" md=\"4\">\r\n        <v-card elevation=\"10\" height=\"300\">\r\n          <v-card-title>\r\n            Premia acciones!\r\n          </v-card-title>\r\n          <v-card-text>\r\n            <v-row>\r\n              <v-col cols=\"4\">\r\n                <v-hover v-slot=\"{ hover }\">\r\n                  <v-card\r\n                    :elevation=\"hover ? 12 : 2\"\r\n                    :class=\"{ 'on-hover': hover }\"\r\n                  >\r\n                    <v-img\r\n                      src=\"../assets/pasion.png\"\r\n                      @click=\"Openvalor('Pasion')\"\r\n                    >\r\n                    </v-img>\r\n                  </v-card>\r\n                </v-hover>\r\n              </v-col>\r\n              <v-col cols=\"4\">\r\n                <v-hover v-slot=\"{ hover }\">\r\n                  <v-card\r\n                    :elevation=\"hover ? 12 : 2\"\r\n                    :class=\"{ 'on-hover': hover }\"\r\n                  >\r\n                    <v-img\r\n                      src=\"../assets/integridad.png\"\r\n                      @click=\"Openvalor('Integridad')\"\r\n                    ></v-img> </v-card\r\n                ></v-hover>\r\n              </v-col>\r\n              <v-col cols=\"4\">\r\n                <v-hover v-slot=\"{ hover }\">\r\n                  <v-card\r\n                    :elevation=\"hover ? 12 : 2\"\r\n                    :class=\"{ 'on-hover': hover }\"\r\n                  >\r\n                    <v-img\r\n                      src=\"../assets/respeto.png\"\r\n                      @click=\"Openvalor('Respeto')\"\r\n                    >\r\n                    </v-img> </v-card\r\n                ></v-hover>\r\n              </v-col>\r\n            </v-row>\r\n            <v-row>\r\n              <v-col> </v-col>\r\n            </v-row>\r\n          </v-card-text>\r\n        </v-card>\r\n      </v-col>\r\n    </v-row>\r\n    <v-row align=\"center\" justify=\"center\">\r\n      <v-col cols=\"12\" xs=\"11\" sm=\"11\" md=\"12\">\r\n        <v-card elevation=\"10\" height=\"300\" style=\"overflow:auto\">\r\n          <v-card-title>\r\n            {{ $t(\"Home.H4\") }}\r\n          </v-card-title>\r\n          <v-card-text>\r\n            <v-data-iterator :items=\"items\" :items-per-page.sync=\"itemsPerPage\">\r\n              <template v-slot:default=\"props\">\r\n                <v-row>\r\n                  <v-col v-for=\"item in props.items\" :key=\"item.name\" cols=\"12\">\r\n                    <v-card>\r\n                      <div class=\"d-flex flex-no-wrap justify-space-between\">\r\n                        <div>\r\n                          <v-card-title v-text=\"item.Titulo\"></v-card-title>\r\n                          <v-card-subtitle\r\n                            v-text=\"item.Cuerpo\"\r\n                          ></v-card-subtitle>\r\n                        </div>\r\n                        <v-avatar class=\"ma-3\" size=\"75\" tile>\r\n                          <v-img src=\"../assets/respeto.png\"></v-img>\r\n                        </v-avatar>\r\n                      </div>\r\n                    </v-card>\r\n                  </v-col>\r\n                </v-row>\r\n              </template>\r\n            </v-data-iterator>\r\n          </v-card-text>\r\n        </v-card>\r\n      </v-col>\r\n    </v-row>\r\n    <v-dialog\r\n      v-model=\"dialogvalores\"\r\n      transition=\"dialog-top-transition\"\r\n      max-width=\"600\"\r\n    >\r\n      <v-card>\r\n        <v-toolbar color=\"primary\" dark>{{ dialogTitle }}</v-toolbar>\r\n        <v-card-text>\r\n          <div class=\"text-h2 pa-12\">\r\n            <v-autocomplete\r\n              v-model=\"EmployeeEmplid\"\r\n              :items=\"Employees\"\r\n              item-value=\"value\"\r\n              item-text=\"text\"\r\n              clearable\r\n              solo-inverted\r\n              label=\"Selecciona al colaborador\"\r\n            ></v-autocomplete>\r\n            <v-textarea\r\n              v-model=\"comments\"\r\n              filled\r\n              counter\r\n              maxlength=\"450\"\r\n              label=\"Porque reconoces al colaborador?\"\r\n            ></v-textarea>\r\n          </div>\r\n        </v-card-text>\r\n        <v-card-actions class=\"justify-end\">\r\n          <v-btn @click=\"dialogvalores = false\" color=\"error\">\r\n            {{ $t(\"Close\") }}\r\n          </v-btn>\r\n          <v-btn @click=\"Savevalor()\" color=\"success\">\r\n            {{ $t(\"Save\") }}\r\n          </v-btn>\r\n        </v-card-actions>\r\n      </v-card>\r\n    </v-dialog>\r\n    <v-snackbar v-model=\"snackbar\" :color=\"snackcolor\">\r\n      {{ snackbarText }}\r\n      <template v-slot:action=\"{ attrs }\">\r\n        <v-btn color=\"white\" text v-bind=\"attrs\" @click=\"snackbar = false\">\r\n          Close\r\n        </v-btn>\r\n      </template>\r\n    </v-snackbar>\r\n  </v-container>\r\n</template>\r\n<script>\r\nimport axios from \"axios\";\r\nexport default {\r\n  name: \"Home\",\r\n  data() {\r\n    const Storeduser = JSON.parse(localStorage.getItem(\"USER_AUTH\"));\r\n    return {\r\n      dialogvalores: false,\r\n      dialogTitle: \"\",\r\n      comments: \"\",\r\n      User: Storeduser,\r\n      EmployeeEmplid: \"\",\r\n      Employees: [],\r\n      snackbar: false,\r\n      snackbarText: \"\",\r\n      snackcolor: \"red\",\r\n      itemsPerPage: 4,\r\n      items: [\r\n        {\r\n          Titulo: \"www\",\r\n          Cuerpo: \"eee\",\r\n          image: \"\",\r\n          Button1: \"\",\r\n          ButtonText1: \"\",\r\n          ButtonColor1: \"\",\r\n          Url1: \"\",\r\n          Button2: \"\",\r\n          ButtonText2: \"\",\r\n          ButtonColor2: \"\",\r\n          Url2: \"\",\r\n        },\r\n      ],\r\n      news: [\r\n        {\r\n          id: \"\",\r\n          Titulo: \"\",\r\n          Cuerpo: \"\",\r\n          Url: \"\",\r\n        },\r\n      ],\r\n    };\r\n  },\r\n  methods: {\r\n    Openvalor(valor) {\r\n      this.dialogvalores = true;\r\n      this.dialogTitle = valor;\r\n    },\r\n    Savevalor() {\r\n      let postada = {\r\n        recibe: this.EmployeeEmplid,\r\n        valor: this.dialogTitle,\r\n        otorga: this.User.emplid,\r\n        comentario: this.comments,\r\n      };\r\n      axios\r\n        .get(\"http://192.168.1.238:92/api/valores/guardavalor.php\", {\r\n          params: postada,\r\n        })\r\n        .then((response) => {\r\n          this.snackbarText = \"Valor asignado\";\r\n          this.snackbar = true;\r\n          this.snackcolor = \"green\";\r\n          this.dialogvalores = false;\r\n        })\r\n        .catch((error) => {\r\n          console.log(response.data);\r\n          this.snackbarText = \"Ocurrió un error al cargar la informacion\";\r\n          this.snackbar = true;\r\n          this.snackcolor = \"red\";\r\n        });\r\n    },\r\n  },\r\n  mounted() {\r\n    //empleados activos\r\n    axios\r\n      .get(\"http://192.168.1.238:92/api/EmployeesActives.php\")\r\n      .then((response) => {\r\n        this.Employees = JSON.parse(JSON.stringify(response.data));\r\n      })\r\n      .catch((error) => {\r\n        console.log(response.data);\r\n      });\r\n    //notificaciones\r\n    let postdata = {\r\n      emplid: this.User.emplid,\r\n    };\r\n    axios\r\n      .get(\"http://192.168.1.238:92/api/notificaciones.php\", {\r\n        params: postdata,\r\n      })\r\n      .then((response) => {\r\n        this.items = JSON.parse(JSON.stringify(response.data));\r\n      })\r\n      .catch((error) => {\r\n        console.log(response.data);\r\n      });\r\n    //Noticias\r\n    axios\r\n      .get(\"http://192.168.1.238:92/api/Noticias/shownews.php\")\r\n      .then((response) => {\r\n        this.news = JSON.parse(JSON.stringify(response.data));\r\n      })\r\n      .catch((error) => {\r\n        console.log(error);\r\n        this.snackbarText = \"Ocurrió un error al cargar las noticias\";\r\n        this.snackbar = true;\r\n        this.loading = false;\r\n      });\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":"src/views"}]}