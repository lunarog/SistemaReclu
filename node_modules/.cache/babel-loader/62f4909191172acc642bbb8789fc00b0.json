sable-sort\r\n                  fixed-header\r\n                  height=\"400px\"\r\n                >\r\n                  <template v-slot:item.Numero=\"{ item }\">\r\n                    <v-btn\r\n                      small\r\n                      color=\"primary\"\r\n                      dark\r\n                      @click=\"\r\n                        $router.push({\r\n                          name: 'Userinfo',\r\n                          params: { emplid: item.Numero,rol:'HJpyl0SY='},\r\n                        })\r\n                      \"\r\n                    >\r\n                      {{ item.Numero }}\r\n                    </v-btn>\r\n                  </template>\r\n                </v-data-table>\r\n              </v-col>\r\n            </v-row>\r\n          </v-card-text>\r\n        </v-card>\r\n      </v-col>\r\n    </v-row>\r\n    <v-snackbar v-model=\"snackbar\" color=\"red\">\r\n      {{ snackbarText }}\r\n      <template v-slot:action=\"{ attrs }\">\r\n        <v-btn color=\"white\" text v-bind=\"attrs\" @click=\"snackbar = false\">\r\n          Close\r\n        </v-btn>\r\n      </template>\r\n    </v-snackbar>\r\n  </v-container>\r\n</template>\r\n<script>\r\nimport Breadcrumbs from \"../../components/Breadcrumbs\";\r\nimport axios from \"axios\";\r\nexport default {\r\n  components: {\r\n    Breadcrumbs,\r\n  },\r\n  data() {\r\n    let content = \"\";\r\n    return {\r\n      page: this.$route.meta.breadcrum[0].name,\r\n      icon: this.$route.meta.breadcrum[0].icon,\r\n      snackbar: false,\r\n      loading: true,\r\n      snackbarText: \"\",\r\n      search: \"\",\r\n      headers: [\r\n        {\r\n          text: \"(#)Numero\",\r\n          align: \"start\",\r\n          sortable: false,\r\n          value: \"Numero\",\r\n          class: \"Headertbldh\",\r\n        },\r\n        { text: \"Nombre Completo\", value: \"Nombre\", class: \"Headertbldh\" },\r\n        { text: \"Area\", value: \"Area\", class: \"Headertbldh\" },\r\n        { text: \"Depto\", value: \"Depto\", class: \"Headertbldh\" },\r\n        { text: \"Responsable\", value: \"Responsable\", class: \"Headertbldh\" },\r\n        { text: \"Tipo\", value: \"Tipo\", class: \"Headertbldh\" },\r\n        { text: \"Proceso\", value: \"Proceso\", class: \"Headertbldh\" },\r\n      ],\r\n      employees: [\r\n        {\r\n          Numero: \"\",\r\n          Nombre: \"\",\r\n          Area: \"\",\r\n          Depto: \"\",\r\n          Puesto: \"\",\r\n          Responsable: \"\",\r\n          Tipo: \"\",\r\n          Proceso: \"\",\r\n        },\r\n      ],\r\n    };\r\n  },\r\n  methods: {},\r\n  mounted() {\r\n    axios\r\n      .get(\"http://192.168.1.238:92/api/Empleados_activos/employees.php\")\r\n      .then((response) => {\r\n        this.employees = JSON.parse(JSON.stringify(response.data));\r\n        this.loading = false;\r\n      })\r\n      .catch((error) => {\r\n        console.log(error);\r\n        this.snackbarText = \"Ocurri√≥ un error al cargar la informacion\";\r\n        this.snackbar = true;\r\n        this.loading = false;\r\n      });\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":"src/views/Administracion"}]}