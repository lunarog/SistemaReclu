AIA,IAJA,CAIA,UAAA,QAAA,EAAA;AACA,QAAA,KAAA,CAAA,QAAA,GAAA,IAAA,CAAA,KAAA,CAAA,IAAA,CAAA,SAAA,CAAA,QAAA,CAAA,IAAA,CAAA,CAAA;AACA,QAAA,KAAA,CAAA,KAAA,GAAA,KAAA,CAAA,QAAA,CAAA,MAAA;AACA,OAPA,EAQA,KARA,CAQA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA;AACA,OAVA;AAWA,KAjBA;AAkBA,IAAA,OAlBA,mBAkBA,CAlBA,EAkBA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AACA,QAAA,IAAA,EAAA,MADA;AAEA,QAAA,MAAA,EAAA;AACA,UAAA,IAAA,EAAA;AADA;AAFA,OAAA;AAMA,KAzBA;AA0BA,IAAA,OA1BA,mBA0BA,CA1BA,EA0BA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AACA,QAAA,IAAA,EAAA,MADA;AAEA,QAAA,MAAA,EAAA;AACA,UAAA,IAAA,EAAA;AADA;AAFA,OAAA;AAMA,KAjCA;AAkCA,IAAA,GAlCA,iBAkCA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,MAAA;AACA;AApCA;AA1BA,CAAA","sourcesContent":["<template>\n  <v-container>\n    <v-sheet>\n      <v-toolbar style=\"background: #00d173\">\n        <v-toolbar-title class=\"d-none d-sm-flex\">\n          <router-link to=\"/\">\n            <v-img\n              max-height=\"60\"\n              max-width=\"140\"\n              src=\"@/assets/logo.png\"\n            ></v-img>\n          </router-link>\n        </v-toolbar-title>\n        <v-row>\n          <v-col cols=\"5\" xs=\"2\" sm=\"4\" md=\"5\">\n            <div style=\"padding: 30px\">\n              <v-text-field\n                v-model=\"que\"\n                label=\"¿Que?\"\n                background-color=\"white\"\n                required\n              ></v-text-field>\n            </div>\n          </v-col>\n          <v-col cols=\"5\" xs=\"2\" sm=\"4\" md=\"5\">\n            <div style=\"padding: 30px\">\n              <v-text-field\n                v-model=\"donde\"\n                label=\"¿Donde?\"\n                background-color=\"white\"\n                required\n              ></v-text-field>\n            </div>\n          </v-col>\n        </v-row>\n        <v-btn @click=\"Get\">Buscar Vacante</v-btn>\n      </v-toolbar>\n    </v-sheet>\n    <v-row>\n      <v-col>\n        <br />\n        <h1>\n          Nuestras vacantes\n          <v-btn @click=\"New\" v-if=\"admin == 1\" dark color=\"#00D173\">\n            Crear nueva vacante\n          </v-btn>\n        </h1>\n        <br />\n        <v-card>\n          <v-card-text>\n\n\n            <h1 v-if=\"total==0\">Lo sentimos, pero no encontramos ninguna vacante disponible.</h1>\n            <v-simple-table v-else>\n              <template v-slot:default>\n                <tbody>\n                  <tr v-for=\"item in Vacantes\" :key=\"item.name\">\n                    <td v-if=\"admin == 1\">\n                      <a style=\"font-size: 26px\" @click=\"Editvac(item.ID)\"\n                        ><i class=\"mdi mdi-pencil-box\"></i\n                      ></a>\n                    </td>\n                    <td @click=\"Showvac(item.ID)\">\n                      <h2>{{ item.VacancyName }}</h2>\n                      <h4>{{ item.Location }}</h4>\n                      {{ item.Description }}\n                    </td>\n                  </tr>\n                </tbody>\n              </template>\n            </v-simple-table>\n          </v-card-text>\n        </v-card>\n      </v-col>\n    </v-row>\n  </v-container>\n</template>\n<script>\nimport axios from \"axios\";\nexport default {\n  name: \"Home\",\n  data() {\n    return {\n      admin: 1,\n      que: \"\",\n      queFlag: \"\",\n      donde: \"\",\n      dondeFlag: \"\",\n      total:0,\n      Vacantes: [\n        {\n          VacancyName: \"\",\n          Description: \"\",\n          VacancyName: \"\",\n          Location: \"\",\n          ID: \"\",\n        },\n      ],\n    };\n  },\n  mounted() {\n    this.que = this.$route.params.filter1;\n    this.donde = this.$route.params.filter2;\n    this.Get();\n  },\n  methods: {\n    Get() {\n      let postdata = {\n        Filter1: this.que,\n        Filter2: this.donde,\n      };\n      axios\n        .get(\"http://localhost/reclu/api/Show.php\", {\n          params: postdata,\n        })\n        .then((response) => {\n          this.Vacantes = JSON.parse(JSON.stringify(response.data));\n          this.total = this.Vacantes.length;\n        })\n        .catch((error) => {\n          console.log(response.data);\n        });\n    },\n    Showvac(x) {\n      this.$router.push({\n        name: \"view\",\n        params: {\n          data: x,\n        },\n      });\n    },\n    Editvac(x) {\n      this.$router.push({\n        name: \"edit\",\n        params: {\n          data: x,\n        },\n      });\n    },\n    New() {\n      this.$router.push(\"/New\");\n    },\n  },\n};\n</script>\n"],"sourceRoot":"src/views"}]}