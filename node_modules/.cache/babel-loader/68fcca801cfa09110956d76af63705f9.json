AAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA;AACA,QAAA,KAAA,CAAA,YAAA,GAAA,2CAAA;AACA,QAAA,KAAA,CAAA,QAAA,GAAA,IAAA;AACA,QAAA,KAAA,CAAA,UAAA,GAAA,KAAA;AACA,OAhBA;AAiBA,KA1BA;AA2BA,IAAA,UA3BA,sBA2BA,KA3BA,EA2BA,EA3BA,EA2BA;AAAA;;AACA,WAAA,QAAA,GAAA,IAAA;AACA,UAAA,MAAA,GAAA,EAAA;AACA,UAAA,KAAA,IAAA,CAAA,EAAA,MAAA,GAAA,CAAA,CAAA,KACA,MAAA,GAAA,CAAA;AACA,UAAA,OAAA,GAAA;AACA,QAAA,OAAA,EAAA,MADA;AAEA,QAAA,EAAA,EAAA;AAFA,OAAA;AAIA,MAAA,KAAA,CACA,GADA,CACA,iDADA,EACA;AACA,QAAA,MAAA,EAAA;AADA,OADA,EAIA,IAJA,CAIA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,YAAA,GAAA,kBAAA;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,IAAA;AACA,QAAA,MAAA,CAAA,UAAA,GAAA,OAAA;AACA,QAAA,MAAA,CAAA,aAAA,GAAA,KAAA;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,KAAA;AACA,OAVA,EAWA,KAXA,CAWA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA;AACA,QAAA,MAAA,CAAA,YAAA,GAAA,2CAAA;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,IAAA;AACA,QAAA,MAAA,CAAA,UAAA,GAAA,KAAA;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,KAAA;AACA,OAjBA;AAkBA;AAtDA,GAjEA;AAyHA,EAAA,OAzHA,qBAyHA;AAAA;;AACA,IAAA,KAAA,CACA,GADA,CACA,+CADA,EAEA,IAFA,CAEA,UAAA,QAAA,EAAA;AACA,MAAA,MAAA,CAAA,IAAA,GAAA,IAAA,CAAA,KAAA,CAAA,IAAA,CAAA,SAAA,CAAA,QAAA,CAAA,IAAA,CAAA,CAAA;AACA,MAAA,MAAA,CAAA,OAAA,GAAA,KAAA;AACA,KALA,EAMA,KANA,CAMA,UAAA,KAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,MAAA,MAAA,CAAA,YAAA,GAAA,2CAAA;AACA,MAAA,MAAA,CAAA,QAAA,GAAA,IAAA;AACA,MAAA,MAAA,CAAA,OAAA,GAAA,KAAA;AACA,KAXA;AAYA,GAtIA;AAuIA,EAAA,UAAA,EAAA;AACA,IAAA,MAAA,EAAA;AADA;AAvIA,CAAA","sourcesContent":["<template>\r\n  <v-container class=\"fill-height\">\r\n    <v-row>\r\n      <Breadcrumbs :page=\"page\" /><br />\r\n      <v-col cols=\"12\" xs=\"12\" sm=\"12\" md=\"12\">\r\n        <h2>\r\n          <p class=\"text-left\">\r\n            <v-icon x-large color=\"blue\">{{ icon }}</v-icon>\r\n            {{ $t(\"News.H1\") }}\r\n          </p>\r\n        </h2>\r\n        <v-card elevation=\"10\">\r\n          <v-card-title>\r\n            <v-text-field\r\n              v-model=\"search\"\r\n              label=\"Buscador\"\r\n              class=\"mx-4\"\r\n            ></v-text-field>\r\n          </v-card-title>\r\n          <v-card-text>\r\n            <v-btn\r\n              color=\"blue\"\r\n              fab\r\n              dark\r\n              small\r\n              absolute\r\n              top\r\n              right\r\n              @click=\"dialog = true\"\r\n            >\r\n              <v-icon>mdi-plus</v-icon>\r\n            </v-btn>\r\n            <v-data-table\r\n              :headers=\"headers\"\r\n              :items=\"news\"\r\n              :search=\"search\"\r\n              :loading=\"loading\"\r\n              :items-per-page=\"5\"\r\n              class=\"elevation-1\"\r\n            >\r\n              <template v-slot:item.Habilitado=\"{ item }\">\r\n                <v-simple-checkbox\r\n                  v-model=\"item.Habilitado\"\r\n                  @click=\"ChangeNews(item.Habilitado,item.Id)\"\r\n                >\r\n                  <template v-slot:label>\r\n                    <v-progress-circular\r\n                      v-show=\"switchMe\"\r\n                      :size=\"20\"\r\n                      indeterminate\r\n                      color=\"primary\"\r\n                    ></v-progress-circular>\r\n                  </template>\r\n                </v-simple-checkbox>\r\n              </template>\r\n            </v-data-table>\r\n          </v-card-text>\r\n        </v-card>\r\n        <v-dialog\r\n          v-model=\"dialog\"\r\n          transition=\"dialog-top-transition\"\r\n          max-width=\"600\"\r\n        >\r\n          <v-card elevation=\"10\">\r\n            <v-card-text>\r\n              <v-row>\r\n                <v-col cols=\"12\" xs=\"12\" sm=\"12\" md=\"12\">\r\n                  <v-form ref=\"form\" v-model=\"valid\" lazy-validation>\r\n                    <v-text-field\r\n                      v-model=\"title\"\r\n                      :rules=\"titleRules\"\r\n                      :label=\"$t('News.Title')\"\r\n                      prepend-icon=\"mdi-comment-outline\"\r\n                      counter\r\n                      maxlength=\"75\"\r\n                      required\r\n                    ></v-text-field>\r\n                    <br />\r\n                    <v-textarea\r\n                      v-model=\"descr\"\r\n                      :rules=\"descrRules\"\r\n                      counter\r\n                      maxlength=\"450\"\r\n                      :label=\"$t('News.Descr')\"\r\n                      prepend-icon=\"mdi-text-box-outline\"\r\n                      required\r\n                    >\r\n                    </v-textarea>\r\n                    <br />\r\n                    <v-text-field\r\n                      v-model=\"image\"\r\n                      :rules=\"imgRules\"\r\n                      counter\r\n                      maxlength=\"150\"\r\n                      prepend-icon=\"mdi-web\"\r\n                      :label=\"$t('News.Pick')\"\r\n                    ></v-text-field>\r\n                    <br />\r\n                    <v-menu\r\n                      ref=\"menu1\"\r\n                      v-model=\"menu1\"\r\n                      :close-on-content-click=\"false\"\r\n                      :return-value.sync=\"date1\"\r\n                      transition=\"scale-transition\"\r\n                      min-width=\"auto\"\r\n                    >\r\n                      <template v-slot:activator=\"{ on, attrs }\">\r\n                        <v-combobox\r\n                          v-model=\"date1\"\r\n                          :label=\"$t('News.Calendar')\"\r\n                          readonly\r\n                          v-bind=\"attrs\"\r\n                          v-on=\"on\"\r\n                          prepend-icon=\"mdi-calendar\"\r\n                        ></v-combobox>\r\n                      </template>\r\n\r\n                      <v-date-picker v-model=\"date1\" no-title scrollable>\r\n                        <v-spacer></v-spacer>\r\n                        <v-btn text color=\"primary\" @click=\"menu1 = false\">\r\n                          Cancel\r\n                        </v-btn>\r\n                        <v-btn\r\n                          text\r\n                          color=\"primary\"\r\n                          @click=\"$refs.menu1.save(date1)\"\r\n                        >\r\n                          OK\r\n                        </v-btn>\r\n                      </v-date-picker>\r\n                    </v-menu>\r\n\r\n                    <v-btn\r\n                      :disabled=\"!valid\"\r\n                      color=\"success\"\r\n                      class=\"mr-4\"\r\n                      @click=\"validate\"\r\n                    >\r\n                      Guardar\r\n                    </v-btn>\r\n                  </v-form>\r\n                </v-col>\r\n              </v-row>\r\n            </v-card-text>\r\n          </v-card>\r\n        </v-dialog>\r\n      </v-col>\r\n    </v-row>\r\n    <v-snackbar v-model=\"snackbar\" color=\"green\">\r\n      {{ snackbarText }}\r\n      <template v-slot:action=\"{ attrs }\">\r\n        <v-btn color=\"white\" text v-bind=\"attrs\" @click=\"snackbar = false\">\r\n          Close\r\n        </v-btn>\r\n      </template>\r\n    </v-snackbar>\r\n  </v-container>\r\n</template>\r\n<script>\r\nimport Breadcrumbs from \"../../components/Breadcrumbs\";\r\nimport { Ripple } from \"vuetify/lib/directives\";\r\nimport axios from \"axios\";\r\nexport default {\r\n  components: {\r\n    Breadcrumbs,\r\n  },\r\n  data() {\r\n    const Storeduser = JSON.parse(localStorage.getItem(\"USER_AUTH\"));\r\n    return {\r\n      valid: true,\r\n      dialog: false,\r\n      page: this.$route.meta.breadcrum[0].name,\r\n      icon: this.$route.meta.breadcrum[0].icon,\r\n      User: Storeduser,\r\n      loading: true,\r\n      search: \"\",\r\n      switchMe: false,\r\n      menu1: false,\r\n      date1: [],\r\n      snackbar: false,\r\n      snackbarText: \"\",\r\n      snackbarColor: \"Red\",\r\n      picker: new Date().toISOString().substr(0, 10),\r\n      title: \"\",\r\n      titleRules: [\r\n        (v) => !!v || \"Title is required\",\r\n        (v) => (v && v.length > 10) || \"Title must be more than 10 characters\",\r\n      ],\r\n      descr: \"\",\r\n      descrRules: [\r\n        (v) => !!v || \"Description is required\",\r\n        (v) =>\r\n          (v && v.length > 10) || \"Description must be more than 25 characters\",\r\n      ],\r\n      image: null,\r\n      imgRules: [\r\n        (v) => !!v || \"Url is required\",\r\n        (v) => (v && v.length > 10) || \"Url must be more than 25 characters\",\r\n      ],\r\n      headers: [\r\n        {\r\n          text: \"(#)ID\",\r\n          align: \"start\",\r\n          sortable: false,\r\n          value: \"Id\",\r\n          class: \"Headertbldh\",\r\n        },\r\n        { text: \"Titulo\", value: \"Titulo\", class: \"Headertbldh\" },\r\n        { text: \"Descripcion\", value: \"Cuerpo\", class: \"Headertbldh\" },\r\n        {\r\n          text: \"Fecha de publicacion\",\r\n          value: \"Fecha_publicacion\",\r\n          class: \"Headertbldh\",\r\n        },\r\n        { text: \"Estatus\", value: \"Habilitado\", class: \"Headertbldh\" },\r\n      ],\r\n      news: [\r\n        {\r\n          Id: \"\",\r\n          Titulo: \"\",\r\n          Cuerpo: \"\",\r\n          Fecha_publicacion: \"\",\r\n          Habilitado: \"\",\r\n        },\r\n      ],\r\n    };\r\n  },\r\n  methods: {\r\n    validate() {\r\n      let postdata = {\r\n        Titulo: this.title,\r\n        Cuerpo: this.descr,\r\n        Imagen: this.image,\r\n        Fecha: this.date1,\r\n        Emplid: this.User.emplid,\r\n      };\r\n      axios\r\n        .get(\"http://192.168.1.238:92/api/Noticias/alta.php\", {\r\n          params: postdata,\r\n        })\r\n        .then((response) => {\r\n          console.log(response.data);\r\n          this.items = JSON.parse(JSON.stringify(response.data));\r\n          this.snackbarText = \"Noticia creada\";\r\n          this.snackbar = true;\r\n          this.snackcolor = \"green\";\r\n        })\r\n        .catch((error) => {\r\n          console.log(response.data);\r\n          this.snackbarText = \"Ocurrió un error al cargar la informacion\";\r\n          this.snackbar = true;\r\n          this.snackcolor = \"red\";\r\n        });\r\n    },\r\n    ChangeNews(event, id) {\r\n      this.switchMe = true;\r\n      let status = \"\";\r\n      if (event == 0) status = 0;\r\n      else status = 1;\r\n      let postada = {\r\n        estatus: status,\r\n        id: id,\r\n      };\r\n      axios\r\n        .get(\"http://192.168.1.238:92/api/Noticias/update.php\", {\r\n          params: postada,\r\n        })\r\n        .then((response) => {\r\n          this.snackbarText = \"Cambio realizado\";\r\n          this.snackbar = true;\r\n          this.snackcolor = \"green\";\r\n          this.dialogvalores = false;\r\n          this.switchMe = false;\r\n        })\r\n        .catch((error) => {\r\n          console.log(response.data);\r\n          this.snackbarText = \"Ocurrió un error al cargar la informacion\";\r\n          this.snackbar = true;\r\n          this.snackcolor = \"red\";\r\n          this.switchMe = false;\r\n        });\r\n    },\r\n  },\r\n  mounted() {\r\n    axios\r\n      .get(\"http://192.168.1.238:92/api/Noticias/show.php\")\r\n      .then((response) => {\r\n        this.news = JSON.parse(JSON.stringify(response.data));\r\n        this.loading = false;\r\n      })\r\n      .catch((error) => {\r\n        console.log(error);\r\n        this.snackbarText = \"Ocurrió un error al cargar la informacion\";\r\n        this.snackbar = true;\r\n        this.loading = false;\r\n      });\r\n  },\r\n  directives: {\r\n    Ripple,\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":"src/views/Administracion"}]}