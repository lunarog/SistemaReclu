AA,IAAA;AACA,SAdA;AAeA;AACA;AA3BA;AApDA,CAAA","sourcesContent":["<template>\n  <v-container class=\"fill-height\">\n    <v-row>\n      <Breadcrumbs :page=\"page\" /><br />\n      <v-col cols=\"12\" xs=\"12\" sm=\"12\" md=\"12\">\n        <h2>\n          <p class=\"text-left\">\n            <v-icon x-large color=\"blue\">{{ icon }}</v-icon>\n            {{ $t(\"NewUser.H1\") }}\n          </p>\n        </h2>\n        <v-card elevation=\"10\">\n          <v-card-text>\n            <v-row>\n              <v-col cols=\"12\" xs=\"12\" sm=\"12\" md=\"12\">\n                <v-radio-group v-model=\"radios\" mandatory row>\n                  <v-radio :label=\"$t('NewUser.Field8')\" :value=\"true\"></v-radio>\n                  <v-radio :label=\"$t('NewUser.Field9')\" :value=\"false\"></v-radio>\n                </v-radio-group>\n                <v-form ref=\"form\" v-model=\"valid\" lazy-validation>\n                  <v-text-field\n                    v-model=\"full\"\n                    :rules=\"nameRules\"\n                    :label=\"$t('NewUser.Field1')\"\n                    required\n                  ></v-text-field>\n                  <v-text-field\n                    v-model=\"email\"\n                    :rules=\"emailRules\"\n                    :label=\"$t('NewUser.Field2')\"\n                    required\n                  ></v-text-field>\n                  <v-text-field\n                    v-model=\"user\"\n                    :rules=\"userRules\"\n                    :label=\"$t('NewUser.Field3')\"\n                    :disabled=\"radios\"\n                  ></v-text-field>\n\n                  <v-text-field\n                    v-model=\"pass\"\n                    :rules=\"passRules\"\n                    :label=\"$t('NewUser.Field4')\"\n                    :disabled=\"radios\"\n                  ></v-text-field>\n\n                  <v-select\n                    v-model=\"profile\"\n                    :items=\"profileItems\"\n                    item-value=\"value\"\n                    item-text=\"text\"\n                    :label=\"$t('NewUser.Field5')\"\n                    data-vv-name=\"selectProfile\"\n                    required\n                  ></v-select>\n\n                  <v-text-field\n                    v-model=\"number\"\n                    :rules=\"numberRules\"\n                    :label=\"$t('NewUser.Field7')\"\n                    required\n                  ></v-text-field>\n\n                  <v-btn\n                    :disabled=\"!valid\"\n                    color=\"success\"\n                    class=\"mr-4\"\n                    @click=\"validate\"\n                  >\n                    Guardar\n                  </v-btn>\n                </v-form>\n              </v-col>\n            </v-row>\n          </v-card-text>\n        </v-card>\n      </v-col>\n    </v-row>\n    <v-snackbar v-model=\"snackbar\" color=\"green\">\n      {{ snackbarText }}\n      <template v-slot:action=\"{ attrs }\">\n        <v-btn color=\"white\" text v-bind=\"attrs\" @click=\"snackbar = false\">\n          Close\n        </v-btn>\n      </template>\n    </v-snackbar>\n  </v-container>\n</template>\n<script>\nimport Breadcrumbs from \"../../components/Breadcrumbs\";\nimport axios from \"axios\";\nexport default {\n  components: {\n    Breadcrumbs,\n  },\n  data() {\n    return {\n      valid: true,\n      radios: false,\n      page: this.$route.meta.breadcrum[0].name,\n      icon: this.$route.meta.breadcrum[0].icon,\n      profile: null,\n      snackbar: false,\n      snackbarText: \"uwu\",\n      profileItems: [\n        { value: \"1\", text: \"Administrador General\" },\n        { value: \"2\", text: \"Administrativo\" },\n        { value: \"4\", text: \"Nominas\" },\n        { value: \"5\", text: \"Enfermeria\" },\n        { value: \"6\", text: \"Empleado\" },\n        { value: \"7\", text: \"Seguridad\" },\n        { value: \"8\", text: \"Responsable\" },\n        { value: \"9\", text: \"Comite\" },\n        { value: \"12\", text: \"Foraneo\" },\n        { value: \"13\", text: \"Foraneo Resp\" },\n        { value: \"15\", text: \"Comedor\" },\n      ],\n      full: \"\",\n      nameRules: [\n        (v) => !!v || \"Name is required\",\n        (v) => (v && v.length > 10) || \"Name must be more than 10 characters\",\n      ],\n      email: \"\",\n      emailRules: [\n        (v) => !!v || \"E-mail is required\",\n        (v) => /.+@.+\\..+/.test(v) || \"E-mail must be valid\",\n      ],\n      user: \"\",\n      userRules: [\n        (v) => !!v || \"Nickname is required\",\n        (v) =>\n          (v && v.length > 5) || \"Nickname must be more than 10 characters\",\n      ],\n      pass: \"\",\n      passRules: [\n        (v) => !!v || \"Password is required\",\n        (v) =>\n          (v && v.length > 10) || \"Password must be more than 10 characters\",\n      ],\n      number: \"\",\n      numberRules: [(v) => !!v || \"Employe number is required\"],\n    };\n  },\n  methods: {\n    validate() {\n      if (this.$refs.form.validate()) {\n        let postdata = {\n          Fullname: this.full,\n          Email: this.email,\n          Username: this.user,\n          Password: this.pass,\n          Profile: this.profile,\n          Num: this.number,\n        };\n        axios\n          .get(\"http://192.168.1.238:92/api/Alta_usuario/Create_user.php\", {\n            params: postdata,\n          })\n          .then((response) => {\n            console.log(response.data);\n            this.snackbarText = response.data;\n            this.snackbar = true;\n          })\n          .catch((error) => {\n            console.log(error);\n            this.snackbarText =\n              \"Ocurrió un error al crear el usuario, contacta al administrador\";\n            this.snackbar = true;\n          });\n      }\n    },\n  },\n};\n</script>\n"],"sourceRoot":"src/views/Administracion"}]}