         class=\"mx-4\"\n            ></v-text-field>\n          </v-card-title>\n          <v-card-text>\n            <v-row>\n              <v-col cols=\"12\" xs=\"12\" sm=\"12\" md=\"12\">\n                <v-data-table\n                  :headers=\"headers\"\n                  :items=\"employees\"\n                  :items-per-page=\"10\"\n                  :search=\"search\"\n                  :loading=\"loading\"\n                  class=\"elevation-1\"\n                  disable-sort\n                  fixed-header\n                  height=\"400px\"\n                >\n                  <template v-slot:item.Numero=\"{ item }\">\n                    <v-btn\n                      small\n                      color=\"primary\"\n                      dark\n                      @click=\"\n                        $router.push({\n                          name: 'Userinfo',\n                          params: { emplid: item.Numero,rol:'HJpyl0SY='},\n                        })\n                      \"\n                    >\n                      {{ item.Numero }}\n                    </v-btn>\n                  </template>\n                </v-data-table>\n              </v-col>\n            </v-row>\n          </v-card-text>\n        </v-card>\n      </v-col>\n    </v-row>\n    <v-snackbar v-model=\"snackbar\" color=\"red\">\n      {{ snackbarText }}\n      <template v-slot:action=\"{ attrs }\">\n        <v-btn color=\"white\" text v-bind=\"attrs\" @click=\"snackbar = false\">\n          Close\n        </v-btn>\n      </template>\n    </v-snackbar>\n  </v-container>\n</template>\n<script>\nimport Breadcrumbs from \"../../components/Breadcrumbs\";\nimport axios from \"axios\";\nexport default {\n  components: {\n    Breadcrumbs,\n  },\n  data() {\n    let content = \"\";\n    return {\n      page: this.$route.meta.breadcrum[0].name,\n      icon: this.$route.meta.breadcrum[0].icon,\n      snackbar: false,\n      loading: true,\n      snackbarText: \"\",\n      search: \"\",\n      headers: [\n        {\n          text: \"(#)Numero\",\n          align: \"start\",\n          sortable: false,\n          value: \"Numero\",\n          class: \"Headertbldh\",\n        },\n        { text: \"Nombre Completo\", value: \"Nombre\", class: \"Headertbldh\" },\n        { text: \"Area\", value: \"Area\", class: \"Headertbldh\" },\n        { text: \"Depto\", value: \"Depto\", class: \"Headertbldh\" },\n        { text: \"Responsable\", value: \"Responsable\", class: \"Headertbldh\" },\n        { text: \"Tipo\", value: \"Tipo\", class: \"Headertbldh\" },\n        { text: \"Proceso\", value: \"Proceso\", class: \"Headertbldh\" },\n      ],\n      employees: [\n        {\n          Numero: \"\",\n          Nombre: \"\",\n          Area: \"\",\n          Depto: \"\",\n          Puesto: \"\",\n          Responsable: \"\",\n          Tipo: \"\",\n          Proceso: \"\",\n        },\n      ],\n    };\n  },\n  methods: {},\n  mounted() {\n    axios\n      .get(\"http://192.168.1.238:92/api/Empleados_activos/employees.php\")\n      .then((response) => {\n        this.employees = JSON.parse(JSON.stringify(response.data));\n        this.loading = false;\n      })\n      .catch((error) => {\n        console.log(error);\n        this.snackbarText = \"Ocurri√≥ un error al cargar la informacion\";\n        this.snackbar = true;\n        this.loading = false;\n      });\n  },\n};\n</script>\n"]}]}