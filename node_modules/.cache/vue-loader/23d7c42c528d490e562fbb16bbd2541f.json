n4\"\n                    :placeholder=\"$t('New.Field10')\"\n                    :rules=\"RulesG\"\n                    required\n                  ></v-text-field></v-col\n                ><v-col\n                  cols=\"4\"\n                  xs=\"4\"\n                  sm=\"4\"\n                  md=\"4\"\n                  style=\"padding-bottom: unset; padding-top: unset\"\n                >\n                  <v-select\n                    v-model=\"ptypeK4\"\n                    :items=\"TypeK\"\n                    item-value=\"value\"\n                    item-text=\"text\"\n                    :label=\"$t('New.Field8')\"\n                  ></v-select></v-col\n              ></v-row>\n              <v-row v-if=\"vshow >= 5\">\n                <v-col\n                  cols=\"5\"\n                  xs=\"5\"\n                  sm=\"5\"\n                  md=\"5\"\n                  style=\"padding-bottom: unset; padding-top: unset\"\n                >\n                  <v-text-field\n                    v-model=\"pcompetencia5\"\n                    :placeholder=\"$t('New.Field9')\"\n                    :rules=\"RulesG\"\n                    required\n                  ></v-text-field></v-col\n                ><v-col\n                  cols=\"3\"\n                  xs=\"3\"\n                  sm=\"3\"\n                  md=\"3\"\n                  style=\"padding-bottom: unset; padding-top: unset\"\n                >\n                  <v-text-field\n                    v-model=\"pponderacion5\"\n                    :placeholder=\"$t('New.Field10')\"\n                    :rules=\"RulesG\"\n                    required\n                  ></v-text-field></v-col\n                ><v-col\n                  cols=\"4\"\n                  xs=\"4\"\n                  sm=\"4\"\n                  md=\"4\"\n                  style=\"padding-bottom: unset; padding-top: unset\"\n                >\n                  <v-select\n                    v-model=\"ptypeK5\"\n                    :items=\"TypeK\"\n                    item-value=\"value\"\n                    item-text=\"text\"\n                    :label=\"$t('New.Field8')\"\n                  ></v-select></v-col\n              ></v-row>\n              <v-btn\n                :disabled=\"!valid\"\n                color=\"success\"\n                class=\"mr-4\"\n                @click=\"Savevac\"\n              >\n                Guardar\n              </v-btn>\n            </v-card-text>\n          </v-form>\n        </v-card>\n      </v-col>\n    </v-row>\n\n    <v-snackbar v-model=\"snackbar\" color=\"green\">\n      {{ snackbarText }}\n      <template v-slot:action=\"{ attrs }\">\n        <v-btn color=\"white\" text v-bind=\"attrs\" @click=\"snackbar = false\">\n          Cerrar\n        </v-btn>\n      </template>\n    </v-snackbar>\n  </v-container>\n</template>\n<script>\nimport axios from \"axios\";\nexport default {\n  components: {},\n  data() {\n    return {\n      filtro1: \"\",\n      filtro2: \"\",\n      valid: true,\n      vshow: 1,\n      page: this.$route.meta.breadcrum[0].name,\n      icon: this.$route.meta.breadcrum[0].icon,\n      snackbar: false,\n      snackbarText: \"\",\n      englishlvlitems: [\n        { value: \"1\", text: \"No aplica\" },\n        { value: \"2\", text: \"Principiante\" },\n        { value: \"3\", text: \"Elemental\" },\n        { value: \"4\", text: \"Pre-intermedio\" },\n        { value: \"5\", text: \"Intermedio avanzado\" },\n        { value: \"6\", text: \"Avanzado\" },\n      ],\n      TypeK: [\n        { value: \"1\", text: \"Competencia Hard\" },\n        { value: \"2\", text: \"Competencia Soft\" },\n      ],\n      statesitems: [\n        { text: \"Aguascalientes\" },\n        { text: \"Baja California\" },\n        { text: \"Baja California Sur\" },\n        { text: \"Campeche\" },\n        { text: \"Chiapas\" },\n        { text: \"Chihuahua\" },\n        { text: \"Coahuila\" },\n        { text: \"Colima\" },\n     