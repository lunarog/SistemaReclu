\n        </h2>\r\n        <v-card elevation=\"10\">\r\n          <v-card-text>\r\n            <v-row>\r\n              <v-col cols=\"12\" xs=\"12\" sm=\"12\" md=\"12\">\r\n                <v-radio-group v-model=\"radios\" mandatory row>\r\n                  <v-radio :label=\"$t('NewUser.Field8')\" :value=\"true\"></v-radio>\r\n                  <v-radio :label=\"$t('NewUser.Field9')\" :value=\"false\"></v-radio>\r\n                </v-radio-group>\r\n                <v-form ref=\"form\" v-model=\"valid\" lazy-validation>\r\n                  <v-text-field\r\n                    v-model=\"full\"\r\n                    :rules=\"nameRules\"\r\n                    :label=\"$t('NewUser.Field1')\"\r\n                    required\r\n                  ></v-text-field>\r\n                  <v-text-field\r\n                    v-model=\"email\"\r\n                    :rules=\"emailRules\"\r\n                    :label=\"$t('NewUser.Field2')\"\r\n                    required\r\n                  ></v-text-field>\r\n                  <v-text-field\r\n                    v-model=\"user\"\r\n                    :rules=\"userRules\"\r\n                    :label=\"$t('NewUser.Field3')\"\r\n                    :disabled=\"radios\"\r\n                  ></v-text-field>\r\n\r\n                  <v-text-field\r\n                    v-model=\"pass\"\r\n                    :rules=\"passRules\"\r\n                    :label=\"$t('NewUser.Field4')\"\r\n                    :disabled=\"radios\"\r\n                  ></v-text-field>\r\n\r\n                  <v-select\r\n                    v-model=\"profile\"\r\n                    :items=\"profileItems\"\r\n                    item-value=\"value\"\r\n                    item-text=\"text\"\r\n                    :label=\"$t('NewUser.Field5')\"\r\n                    data-vv-name=\"selectProfile\"\r\n                    required\r\n                  ></v-select>\r\n\r\n                  <v-text-field\r\n                    v-model=\"number\"\r\n                    :rules=\"numberRules\"\r\n                    :label=\"$t('NewUser.Field7')\"\r\n                    required\r\n                  ></v-text-field>\r\n\r\n                  <v-btn\r\n                    :disabled=\"!valid\"\r\n                    color=\"success\"\r\n                    class=\"mr-4\"\r\n                    @click=\"validate\"\r\n                  >\r\n                    Guardar\r\n                  </v-btn>\r\n                </v-form>\r\n              </v-col>\r\n            </v-row>\r\n          </v-card-text>\r\n        </v-card>\r\n      </v-col>\r\n    </v-row>\r\n    <v-snackbar v-model=\"snackbar\" color=\"green\">\r\n      {{ snackbarText }}\r\n      <template v-slot:action=\"{ attrs }\">\r\n        <v-btn color=\"white\" text v-bind=\"attrs\" @click=\"snackbar = false\">\r\n          Close\r\n        </v-btn>\r\n      </template>\r\n    </v-snackbar>\r\n  </v-container>\r\n</template>\r\n<script>\r\nimport Breadcrumbs from \"../../components/Breadcrumbs\";\r\nimport axios from \"axios\";\r\nexport default {\r\n  components: {\r\n    Breadcrumbs,\r\n  },\r\n  data() {\r\n    return {\r\n      valid: true,\r\n      radios: false,\r\n      page: this.$route.meta.breadcrum[0].name,\r\n      icon: this.$route.meta.breadcrum[0].icon,\r\n      profile: null,\r\n      snackbar: false,\r\n      snackbarText: \"uwu\",\r\n      profileItems: [\r\n        { value: \"1\", text: \"Administrador General\" },\r\n        { value: \"2\", text: \"Administrativo\" },\r\n        { value: \"4\", text: \"Nominas\" },\r\n        { value: \"5\", text: \"Enfermeria\" },\r\n        { value: \"6\", text: \"Empleado\" },\r\n        { value: \"7\", text: \"Seguridad\" },\r\n        { value: \"8\", text: \"Responsable\" },\r\n        { value: \"9\", text: \"Comite\" },\r\n        { value: \"12\", text: \"Foraneo\" },\r\n        { value: \"13\", text: \"Foraneo Resp\" },\r\n        { value: \"15\", text: \"Comedor\" },\r\n      ],\r\n      full: \"\",\r\n      nameRules: [\r\n        (v) => !!v || \"Name is required\",\r\n        (v) => (v && v.length > 10) || \"Name must be more than 10 characters\",\r\n      ],\r\n      email: \"\",\r\n      emailRules: [\r\n        (v) => !!v || \"E-mail is required\",\r\n        (v) => /.+@.+\\..+/.test(v) || \"E-mail must be valid\",\r\n      ],\r\n      user: \"\",\r\n      userRules: [\r\n        (v) => !!v || \"Nickname is required\",\r\n        (v) =>\r\n          (v && v.length > 5) || \"Nickname must be more than 10 characters\",\r\n      ],\r\n      pass: \"\",\r\n      passRules: [\r\n        (v) => !!v || \"Password is required\",\r\n        (v) =>\r\n          (v && v.length > 10) || \"Password must be more than 10 characters\",\r\n      ],\r\n      number: \"\",\r\n      numberRules: [(v) => !!v || \"Employe number is required\"],\r\n    };\r\n  },\r\n  methods: {\r\n    validate() {\r\n      if (this.$refs.form.validate()) {\r\n        let postdata = {\r\n          Fullname: this.full,\r\n          Email: this.email,\r\n          Username: this.user,\r\n          Password: this.pass,\r\n          Profile: this.profile,\r\n          Num: this.number,\r\n        };\r\n        axios\r\n          .get(\"http://192.168.1.238:92/api/Alta_usuario/Create_user.php\", {\r\n            params: postdata,\r\n          })\r\n          .then((response) => {\r\n            console.log(response.data);\r\n            this.snackbarText = response.data;\r\n            this.snackbar = true;\r\n          })\r\n          .catch((error) => {\r\n            console.log(error);\r\n            this.snackbarText =\r\n              \"Ocurri√≥ un error al crear el usuario, contacta al administrador\";\r\n            this.snackbar = true;\r\n          });\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n"]}]}