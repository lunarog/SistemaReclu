usuario.Area\") }}</b></v-col\r\n                    >\r\n                    <v-col md=\"4\">{{ usuariof.area }}</v-col>\r\n                  </v-row>\r\n                  <v-row no-gutters>\r\n                    <v-col md=\"3\"\r\n                      ><b>{{ $t(\"Pefil-usuario.Responsable\") }}</b></v-col\r\n                    >\r\n                    <v-col md=\"4\">{{ usuariof.responsable }}</v-col>\r\n                  </v-row>\r\n                  <v-row no-gutters>\r\n                    <v-col md=\"3\"\r\n                      ><b>{{ $t(\"Pefil-usuario.IMSS\") }}</b></v-col\r\n                    >\r\n                    <v-col md=\"4\">{{ usuariof.imss }}</v-col>\r\n                  </v-row>\r\n                  <v-row no-gutters>\r\n                    <v-col md=\"3\"\r\n                      ><b>{{ $t(\"Pefil-usuario.RFC\") }}</b></v-col\r\n                    >\r\n                    <v-col md=\"4\">{{ usuariof.rfc }}</v-col>\r\n                  </v-row>\r\n                  <v-row no-gutters>\r\n                    <v-col md=\"3\"\r\n                      ><b>{{ $t(\"Pefil-usuario.CURP\") }}</b></v-col\r\n                    >\r\n                    <v-col md=\"4\">{{ usuariof.curp }}</v-col>\r\n                  </v-row>\r\n                </v-tab-item>\r\n                <v-tab-item key=\"turnos\">\r\n                  <v-row>\r\n                    <v-col md=\"3\"\r\n                      ><b>{{ $t(\"Pefil-usuario.Grupo\") }}</b></v-col\r\n                    >\r\n                    <v-col md=\"4\">\r\n                      <v-select\r\n                        v-model=\"grupo\"\r\n                        :items=\"grupos\"\r\n                        item-value=\"value\"\r\n                        item-text=\"text\"\r\n                        :label=\"$t('Pefil-usuario.Grupo')\"\r\n                      ></v-select>\r\n                    </v-col>\r\n                  </v-row>\r\n\r\n                  <v-row>\r\n                    <v-col md=\"3\"\r\n                      ><b>{{ $t(\"Pefil-usuario.Turno\") }}</b></v-col\r\n                    >\r\n                    <v-col md=\"4\">\r\n                      <v-select\r\n                        v-model=\"horario\"\r\n                        :items=\"horarios\"\r\n                        item-value=\"value\"\r\n                        item-text=\"text\"\r\n                        :label=\"$t('Pefil-usuario.Turno')\"\r\n                      ></v-select>\r\n                    </v-col>\r\n                  </v-row>\r\n\r\n                  <v-row>\r\n                    <v-col md=\"3\">\r\n                      <v-btn\r\n                        small\r\n                        color=\"success\"\r\n                        style=\"width: 100%\"\r\n                        @click=\"ChangeT()\"\r\n                      >\r\n                        {{ $t(\"UserInfo.Savec\") }}\r\n                      </v-btn>\r\n                    </v-col>\r\n                  </v-row>\r\n                </v-tab-item>\r\n              </v-tabs-items>\r\n            </v-container>\r\n          </v-card-text>\r\n        </v-card>\r\n      </v-col>\r\n    </v-row>\r\n    <v-dialog\r\n      v-model=\"dialogFired\"\r\n      fullscreen\r\n      hide-overlay\r\n      transition=\"dialog-bottom-transition\"\r\n      scrollable\r\n    >\r\n      <v-card tile>\r\n        <v-toolbar flat dark color=\"primary\">\r\n          <v-btn icon dark @click=\"dialogFired = false\">\r\n            <v-icon>mdi-close</v-icon>\r\n          </v-btn>\r\n          <v-toolbar-title>{{ $t(\"UserInfo.Field2\") }}</v-toolbar-title>\r\n        </v-toolbar>\r\n        <v-card-text>\r\n          <v-form ref=\"form\" v-model=\"valid\" lazy-validation>\r\n            <v-menu\r\n              ref=\"menu1\"\r\n              v-model=\"menu1\"\r\n              :close-on-content-click=\"false\"\r\n              :return-value.sync=\"date1\"\r\n              transition=\"scale-transition\"\r\n              offset-y\r\n              min-width=\"auto\"\r\n            >\r\n              <template v-slot:activator=\"{ on, attrs }\">\r\n                <v-combobox\r\n                  v-model=\"date1\"\r\n                  :label=\"$t('UserInfo.Field3')\"\r\n                  readonly\r\n                  v-bind=\"attrs\"\r\n                  v-on=\"on\"\r\n                ></v-combobox>\r\n              </template>\r\n              <v-date-picker v-model=\"date1\" multiple no-title scrollable>\r\n                <v-spacer></v-spacer>\r\n                <v-btn text color=\"primary\" @click=\"menu1 = false\">\r\n                  Cancel\r\n                </v-btn>\r\n                <v-btn text color=\"primary\" @click=\"$refs.menu1.save(date1)\">\r\n                  OK\r\n                </v-btn>\r\n              </v-date-picker>\r\n            </v-menu>\r\n\r\n            <v-menu\r\n              ref=\"menu2\"\r\n              v-model=\"menu2\"\r\n              :close-on-content-click=\"false\"\r\n              :return-value.sync=\"date2\"\r\n              transition=\"scale-transition\"\r\n              offset-y\r\n              min-width=\"auto\"\r\n            >\r\n              <template v-slot:activator=\"{ on, attrs }\">\r\n                <v-combobox\r\n                  v-model=\"date2\"\r\n                  :label=\"$t('UserInfo.Field4')\"\r\n                  readonly\r\n                  v-bind=\"attrs\"\r\n                  v-on=\"on\"\r\n                ></v-combobox>\r\n              </template>\r\n              <v-date-picker v-model=\"date2\" multiple no-title scrollable>\r\n                <v-spacer></v-spacer>\r\n                <v-btn text color=\"primary\" @click=\"menu2 = false\">\r\n                  Cancel\r\n                </v-btn>\r\n                <v-btn text color=\"primary\" @click=\"$refs.menu2.save(date2)\">\r\n                  OK\r\n                </v-btn>\r\n              </v-date-picker>\r\n            </v-menu>\r\n\r\n            <v-select\r\n              v-model=\"general\"\r\n              :items=\"generalItems\"\r\n              item-value=\"value\"\r\n              item-text=\"text\"\r\n              :label=\"$t('UserInfo.Field5')\"\r\n              required\r\n            ></v-select>\r\n\r\n            <v-select\r\n              v-model=\"sub1\"\r\n              :items=\"sub1Items\"\r\n              item-value=\"value\"\r\n              item-text=\"text\"\r\n              :label=\"$t('UserInfo.Field6')\"\r\n              @change=\"onChange()\"\r\n              required\r\n            ></v-select>\r\n\r\n            <v-select\r\n              v-model=\"sub2\"\r\n              :items=\"sub2Items\"\r\n              item-value=\"value\"\r\n              item-text=\"text\"\r\n              :label=\"$t('UserInfo.Field7')\"\r\n              required\r\n            ></v-select>\r\n\r\n            <v-select\r\n              v-model=\"estatus\"\r\n              :items=\"estatusItems\"\r\n              item-value=\"value\"\r\n              item-text=\"text\"\r\n              :label=\"$t('UserInfo.Field8')\"\r\n              data-vv-name=\"selectProfile\"\r\n              required\r\n            ></v-select>\r\n\r\n            <v-text-field\r\n              v-model=\"coments\"\r\n              :rules=\"comentsRules\"\r\n              :label=\"$t('UserInfo.Field9')\"\r\n              required\r\n            ></v-text-field>\r\n            <v-btn\r\n              :disabled=\"!valid\"\r\n              color=\"success\"\r\n              class=\"mr-4\"\r\n              @click=\"Save()\"\r\n            >\r\n              {{ $t(\"UserInfo.Field10\") }}\r\n            </v-btn>\r\n          </v-form>\r\n        </v-card-text>\r\n        <div style=\"flex: 1 1 auto;\"></div>\r\n      </v-card>\r\n    </v-dialog>\r\n    <v-snackbar v-model=\"snackbar\" :color=\"snackcolor\">\r\n      {{ snackbarText }}\r\n      <template v-slot:action=\"{ attrs }\">\r\n        <v-btn color=\"white\" text v-bind=\"attrs\" @click=\"snackbar = false\">\r\n          Close\r\n        </v-btn>\r\n      </template>\r\n    </v-snackbar>\r\n  </v-container>\r\n</template>\r\n<script>\r\nimport Breadcrumbs from \"../../components/Breadcrumbs\";\r\nimport { mapActions, mapState } from \"vuex\";\r\nimport axios from \"axios\";\r\nexport default {\r\n  components: {\r\n    Breadcrumbs,\r\n  },\r\n  data() {\r\n    return {\r\n      Tab: null,\r\n      dialogFired: false,\r\n      menu1: false,\r\n      menu2: false,\r\n      date1: [],\r\n      date2: [],\r\n      valid: true,\r\n      page: this.$route.meta.breadcrum[0].name,\r\n      icon: this.$route.meta.breadcrum[0].icon,\r\n      rol: this.$route.params.rol,\r\n      snackbar: false,\r\n      snackbarText: \"\",\r\n      snackcolor: \"red\",\r\n      usuariof: {\r\n        emplid: \"\",\r\n        name: \"\",\r\n        ingreso: \"\",\r\n        puesto: \"\",\r\n        depto: \"\",\r\n        area: \"\",\r\n        responsable: \"\",\r\n        imss: \"\",\r\n        rfc: \"\",\r\n        curp: \"\",\r\n        img: \"\",\r\n      },\r\n      general: \"\",\r\n      generalItems: [],\r\n      sub1: \"\",\r\n      sub1Items: [],\r\n      sub2: \"\",\r\n      sub2Items: [],\r\n      horario: \"\",\r\n      horarios: [],\r\n      grupo: \"\",\r\n      grupos: [],\r\n      estatus: \"\",\r\n      estatusItems: [\r\n        { text: \"Verde\", value: 1 },\r\n        { text: \"Rojo\", value: 0 },\r\n        { text: \"Amarillo\", value: 2 },\r\n      ],\r\n      coments: \"\",\r\n      comentsRules: [\r\n        (v) => !!v || \"Coments is required\",\r\n        (v) => (v && v.length > 10) || \"Name must be more than 10 characters\",\r\n      ],\r\n    };\r\n  },\r\n  methods: {\r\n    ...mapActions([\"loginAs\"]),\r\n    onChange() {\r\n      let postdata4 = {\r\n        Opc: 4,\r\n        idbaja: this.sub1,\r\n      };\r\n      axios\r\n        .get(\"http://192.168.1.238:92/api/Empleados_activos/info.php\", {\r\n          params: postdata4,\r\n        })\r\n       