                          :close-on-content-click=\"false\"\n                                    :nudge-right=\"40\"\n                                    transition=\"scale-transition\"\n                                    offset-y\n                                    min-width=\"290px\"\n                                >\n                                    <template v-slot:activator=\"{ on, attrs }\">\n                                    <v-text-field\n                                        v-model=\"date\"\n                                        label=\"Entrada\"\n                                        prepend-icon=\"mdi-calendar\"\n                                        readonly\n                                        v-bind=\"attrs\"\n                                        v-on=\"on\"\n                                    ></v-text-field>\n                                    </template>\n                                    <v-date-picker\n                                    v-model=\"date\"\n                                    @input=\"menu1 = false\"\n                                    ></v-date-picker>\n                                </v-menu>\n                            </v-col>\n                            <v-col cols=\"6\" >\n                               <v-menu\n                                    v-model=\"menu2\"\n                                    :close-on-content-click=\"false\"\n                                    :nudge-right=\"40\"\n                                    transition=\"scale-transition\"\n                                    offset-y\n                                    min-width=\"290px\"\n                                >\n                                    <template v-slot:activator=\"{ on, attrs }\">\n                                    <v-text-field\n                                        v-model=\"date\"\n                                        label=\"Salida\"\n                                        prepend-icon=\"mdi-calendar\"\n                                        readonly\n                                        v-bind=\"attrs\"\n                                        v-on=\"on\"\n                                    ></v-text-field>\n                                    </template>\n                                    <v-date-picker\n                                    v-model=\"date\"\n                                    @input=\"menu2 = false\"\n                                    ></v-date-picker>\n                                </v-menu>\n                            </v-col>\n                        </v-row>\n                        <v-row>\n                            <v-col cols=\"6\" >\n                                <v-text-field\n                                label=\"Nombre completo\"\n                                :rules=\"rules\"\n                                hide-details=\"auto\"\n                                ></v-text-field>\n                            </v-col>\n                            <v-col cols=\"6\" >\n                                <v-text-field\n                                label=\"Correo\"\n                                :rules=\"rules\"\n                                hide-details=\"auto\"\n                                ></v-text-field>\n                            </v-col>\n                        </v-row>\n                        <v-row>\n                            <v-col cols=\"6\" >\n                                <v-select\n                                :items=\"items\"\n                                label=\"Tipo de visita\"\n                                dense\n                                ></v-select>\n                            </v-col>\n                        </v-row>\n                        <v-row>\n                            <v-col cols=\"12\">\n                                <v-textarea\n                                color=\"teal\"\n                                rows=\"2\"\n                                >\n                                <template v-slot:label>\n                                    <div>\n                                    Razon de la visita\n                                    </div>\n                                </template>\n                                </v-textarea>\n                            </v-col>\n                        </v-row><v-row>\n                            <v-col cols=\"12\">\n                                <v-textarea\n                                color=\"teal\"\n                                rows=\"2\"\n                                >\n                                <template v-slot:label>\n                                    <div>\n                                    Comentarios <small>(Opcional)</small>\n                                    </div>\n                                </template>\n                                </v-textarea>\n                            </v-col>\n                        </v-row>\n                    </v-card-text>\n                    <v-card-actions>\n                        <v-btn tile color=\"success\" >\n                            <v-icon left>\n                                mdi-content-save\n                            </v-icon>\n                            Guardar visita\n                        </v-btn>\n                    </v-card-actions>\n                </v-card>\n            </v-col>\n            <v-col cols=\"12\" xs=\"12\" md=\"4\">\n                <v-card class=\"mx-auto\" outlined >\n                    <v-card-title>\n                        Visitas frecuentes\n                    </v-card-title>\n                    <v-card-subtitle>\n                        Cargar informacion de visitantes frecuentes\n                    </v-card-subtitle>\n                </v-card>\n            </v-col>\n        </v-row>\n    </v-container>\n</template>\n<script>\n  export default {\n    data: () => ({\n      date: new Date().toISOString().substr(0, 10),\n      menu1: false,\n      menu2: false,\n      rules: [\n        value => !!value || 'Campo requerido.',\n        value => (value && value.length >= 3) || 'Min 3 characters',\n      ],\n        items: ['Reclutamiento', 'Proveedor', 'Cliente', 'Capacitacion','Direccion general'],\n\n    }),\n  }\n</script>"]}]}