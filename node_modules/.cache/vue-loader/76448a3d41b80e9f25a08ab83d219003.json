v-icon>\r\n            {{ $t(\"Calendar.H1\") }}\r\n          </p>\r\n        </h2>\r\n        <v-card elevation=\"10\">\r\n          <v-card-title>\r\n            <v-toolbar flat>\r\n              <v-btn\r\n                outlined\r\n                class=\"mr-4\"\r\n                color=\"grey darken-2\"\r\n                @click=\"setToday\"\r\n              >\r\n                {{ $t(\"Calendar.Today\") }}\r\n              </v-btn>\r\n              <v-btn fab text small color=\"grey darken-2\" @click=\"prev\">\r\n                <v-icon small>\r\n                  mdi-chevron-left\r\n                </v-icon>\r\n              </v-btn>\r\n              <v-btn fab text small color=\"grey darken-2\" @click=\"next\">\r\n                <v-icon small>\r\n                  mdi-chevron-right\r\n                </v-icon>\r\n              </v-btn>\r\n              <v-toolbar-title v-if=\"$refs.calendar\">\r\n                {{ $refs.calendar.title }}\r\n              </v-toolbar-title>\r\n              <v-spacer></v-spacer>\r\n            </v-toolbar>\r\n          </v-card-title>\r\n          <v-card-text>\r\n            <v-sheet height=\"675\">\r\n              <v-calendar\r\n                ref=\"calendar\"\r\n                v-model=\"focus\"\r\n                :type=\"type\"\r\n                :events=\"events\"\r\n                :event-overlap-threshold=\"30\"\r\n                :event-color=\"getEventColor\"\r\n                @change=\"GetCalendar\"\r\n                height=\"70px\"\r\n              ></v-calendar>\r\n            </v-sheet>\r\n          </v-card-text>\r\n        </v-card>\r\n      </v-col>\r\n    </v-row>\r\n  </v-container>\r\n</template>\r\n<script>\r\nimport Breadcrumbs from \"../../components/Breadcrumbs\";\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  components: {\r\n    Breadcrumbs,\r\n  },\r\n  data() {\r\n    const Storeduser = JSON.parse(localStorage.getItem(\"USER_AUTH\"));\r\n    return {\r\n      page: this.$route.meta.breadcrum[0].name,\r\n      icon: this.$route.meta.breadcrum[0].icon,\r\n      User: Storeduser,\r\n      focus: \"\",\r\n      type: \"month\",\r\n      typeToLabel: {\r\n        month: \"Month\",\r\n        week: \"Week\",\r\n        day: \"Day\",\r\n        \"4day\": \"4 Days\",\r\n      },\r\n      selectedEvent: {},\r\n      selectedElement: null,\r\n      selectedOpen: false,\r\n      events: [],\r\n      colors: [\"blue\", \"indigo\", \"deep-purple\", \"cyan\", \"green\"],\r\n      names: [\r\n        \"Vacaciones\",\r\n        \"Permiso especial\",\r\n        \"Home Ofice\",\r\n        \"Salida anticipada\",\r\n        \"Llegada tarde\",\r\n        \"Tiempo x tiempo\",\r\n      ],\r\n    };\r\n  },\r\n  mounted() {\r\n    this.GetCalendar();\r\n  },\r\n  methods: {\r\n    viewDay({ date }) {\r\n      this.focus = date;\r\n      this.type = \"day\";\r\n    },\r\n    getEventColor(event) {\r\n      return event.color;\r\n    },\r\n    setToday() {\r\n      this.focus = \"\";\r\n    },\r\n    prev() {\r\n      this.$refs.calendar.prev();\r\n    },\r\n    next() {\r\n      this.$refs.calendar.next();\r\n    },\r\n    showEvent({ nativeEvent, event }) {\r\n      const open = () => {\r\n        this.selectedEvent = event;\r\n        this.selectedElement = nativeEvent.target;\r\n        setTimeout(() => {\r\n          this.selectedOpen = true;\r\n        }, 10);\r\n      };\r\n\r\n      if (this.selectedOpen) {\r\n        this.selectedOpen = false;\r\n        setTimeout(open, 10);\r\n      } else {\r\n        open();\r\n      }\r\n\r\n      nativeEvent.stopPropagation();\r\n    },\r\n\r\n    GetCalendar() {\r\n      let postdata = {\r\n        emplid: this.User.emplid,\r\n      };\r\n      axios\r\n        .get(\"http://192.168.1.238:92/api/Calendario/showEvents.php\", {\r\n          params: postdata,\r\n        })\r\n        .then((response) => {\r\n          this.events = JSON.parse(JSON.stringify(response.data));\r\n        })\r\n        .catch((error) => {\r\n          console.log(response.data);\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n.blue{\r\n color:black !important;   \r\n}\r\n</style>"]}]}