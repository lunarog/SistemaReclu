tion>\n            <v-card-text>\n              <v-row>\n                <v-col cols=\"12\" xs=\"12\" sm=\"12\" md=\"12\">\n                  <v-row>\n                    <v-col cols=\"12\" xs=\"12\" sm=\"12\" md=\"12\">\n                      <v-text-field\n                        v-model=\"Vacantes.VacancyName\"\n                        :label=\"$t('New.Field1')\"\n                        counter\n                        maxlength=\"100\"\n                        :rules=\"RulesG\"\n                        required\n                      ></v-text-field>\n                      <v-textarea\n                        v-model=\"Vacantes.Description\"\n                        :rules=\"RulesG\"\n                        :label=\"$t('New.Field11')\"\n                        rows=\"2\"\n                        counter\n                        maxlength=\"2500\"\n                        required\n                      ></v-textarea>\n                      <v-select\n                        v-model=\"Vacantes.Location\"\n                        :items=\"statesitems\"\n                        :label=\"$t('New.Field2')\"\n                        :rules=\"[(v) => !!v || 'Campo requerido']\"\n                        required\n                      ></v-select>\n                      <v-switch\n                        v-model=\"Vacantes.ShowSalary\"\n                        label=\"Mostrar salario\"\n                      ></v-switch>\n                      <v-text-field\n                        v-model=\"Vacantes.MinSalary\"\n                        :rules=\"RulesG\"\n                        :label=\"$t('New.Field3')\"\n                        required\n                      ></v-text-field>\n                      <v-text-field\n                        v-model=\"Vacantes.MaxSalary\"\n                        :rules=\"RulesG\"\n                        :label=\"$t('New.Field4')\"\n                        required\n                      ></v-text-field>\n                    </v-col>\n                  </v-row>\n                </v-col>\n              </v-row>\n              <v-row>\n                <v-col>\n                  <v-select\n                    v-model=\"Vacantes.EnglishLevel\"\n                    :items=\"englishlvlitems\"\n                    item-value=\"value\"\n                    item-text=\"text\"\n                    :label=\"$t('New.Field5')\"\n                    :rules=\"[(v) => !!v || 'Campo requerido']\"\n                    required\n                  ></v-select>\n                </v-col>\n                <v-col>\n                  <v-rating\n                    empty-icon=\"\"\n                    color=\"green\"\n                    full-icon=\"mdi-color-helper\"\n                    length=\"5\"\n                    size=\"35\"\n                    :value=\"Vacantes.EnglishLevel\"\n                    required\n                  ></v-rating>\n                </v-col>\n              </v-row>\n              <v-row>\n                <v-col>\n                  <v-row>\n                    <v-col>\n                      <v-textarea\n                        v-model=\"Vacantes.LaborExperience\"\n                        :label=\"$t('New.Field6')\"\n                        rows=\"2\"\n                        counter\n                        maxlength=\"250\"\n                        :rules=\"RulesG\"\n                        required\n                      ></v-textarea>\n                    </v-col>\n                  </v-row>\n                  <v-row>\n                    <v-col>\n                      <v-textarea\n                        v-model=\"Vacantes.Scholarship\"\n                        :label=\"$t('New.Field7')\"\n                        rows=\"2\"\n                        counter\n                        maxlength=\"250\"\n                        :rules=\"RulesG\"\n                        required\n                      ></v-textarea>\n                    </v-col>\n                  </v-row>\n                </v-col>\n              </v-row>\n              <v-row>\n                <v-col cols=\"10\" xs=\"10\" sm=\"10\" md=\"10\">\n                  <label style=\"font-size: 18px\">Conocimientos</label>\n                </v-col>\n                <v-col cols=\"1\" xs=\"1\" sm=\"1\" md=\"1\">\n                  <a\n                    ><i\n                      style=\"font-size: 28px\"\n                      @click=\"Addmore(1)\"\n                      class=\"mdi mdi-minus\"\n                    ></i\n                  ></a>\n                </v-col>\n                <v-col cols=\"1\" xs=\"1\" sm=\"1\" md=\"1\">\n                  <a\n                    ><i\n                      style=\"font-size: 28px\"\n                      @click=\"Addmore(2)\"\n                      class=\"mdi mdi-plus\"\n                    ></i\n                  ></a>\n                </v-col>\n              </v-row>\n              <v-row>\n                <v-col\n                  cols=\"5\"\n                  xs=\"5\"\n                  sm=\"5\"\n                  md=\"5\"\n                  style=\"padding-bottom: unset; padding-top: unset\"\n                >\n                  <v-text-field\n                    v-model=\"Perfil[0].Description\"\n                    :placeholder=\"$t('New.Field9')\"\n                    :rules=\"RulesG\"\n                    required\n                  ></v-text-field></v-col\n                ><v-col\n                  cols=\"3\"\n                  xs=\"3\"\n                  sm=\"3\"\n                  md=\"3\"\n                  style=\"padding-bottom: unset; padding-top: unset\"\n                >\n                  <v-text-field\n                    v-model=\"Perfil[0].Percentage\"\n                    :placeholder=\"$t('New.Field10')\"\n                    :rules=\"RulesG\"\n                    required\n                  ></v-text-field></v-col\n                ><v-col\n                  cols=\"4\"\n                  xs=\"4\"\n                  sm=\"4\"\n                  md=\"4\"\n                  style=\"padding-bottom: unset; padding-top: unset\"\n                >\n                  <v-select\n                    v-model=\"ptypeK1\"\n                    :items=\"TypeK\"\n                    item-value=\"value\"\n                    item-text=\"text\"\n                    :label=\"$t('New.Field8')\"\n                  ></v-select></v-col\n              ></v-row>\n              <v-row v-if=\"vshow >= 2\">\n                <v-col\n                  cols=\"5\"\n                  xs=\"5\"\n                  sm=\"5\"\n                  md=\"5\"\n                  style=\"padding-bottom: unset; padding-top: unset\"\n                >\n                  <v-text-field\n                    v-model=\"Perfil[1].Description\"\n                    :placeholder=\"$t('New.Field9')\"\n                    :rules=\"RulesG\"\n                    required\n                  ></v-text-field></v-col\n                ><v-col\n                  cols=\"3\"\n                  xs=\"3\"\n                  sm=\"3\"\n                  md=\"3\"\n                  style=\"padding-bottom: unset; padding-top: unset\"\n                >\n                  <v-text-field\n                    v-model=\"Perfil[1].Percentage\"\n                    :placeholder=\"$t('New.Field10')\"\n                    :rules=\"RulesG\"\n                    required\n                  ></v-text-field></v-col\n                ><v-col\n                  cols=\"4\"\n                  xs=\"4\"\n                  sm=\"4\"\n                  md=\"4\"\n                  style=\"padding-bottom: unset; padding-top: unset\"\n                >\n                  <v-select\n                    v-model=\"ptypeK2\"\n                    :items=\"TypeK\"\n                    item-value=\"value\"\n                    item-text=\"text\"\n                    :label=\"$t('New.Field8')\"\n                  ></v-select></v-col\n              ></v-row>\n              <v-row v-if=\"vshow >= 3\">\n                <v-col\n                  cols=\"5\"\n                  xs=\"5\"\n                  sm=\"5\"\n                  md=\"5\"\n                  style=\"padding-bottom: unset; padding-top: unset\"\n                >\n                  <v-text-field\n                    v-model=\"Perfil[2].Description\"\n                    :placeholder=\"$t('New.Field9')\"\n                    :rules=\"RulesG\"\n                    required\n                  ></v-text-field></v-col\n                ><v-col\n                  cols=\"3\"\n                  xs=\"3\"\n                  sm=\"3\"\n                  md=\"3\"\n                  style=\"padding-bottom: unset; padding-top: unset\"\n                >\n                  <v-text-field\n                    v-model=\"Perfil[2].Percentage\"\n                    :placeholder=\"$t('New.Field10')\"\n                    :rules=\"RulesG\"\n                    required\n                  ></v-text-field></v-col\n                ><v-col\n                  cols=\"4\"\n                  xs=\"4\"\n                  sm=\"4\"\n                  md=\"4\"\n                  style=\"padding-bottom: unset; padding-top: unset\"\n                >\n                  <v-select\n                    v-model=\"ptypeK3\"\n                    :items=\"TypeK\"\n                    item-value=\"value\"\n                    item-text=\"text\"\n                    :label=\"$t('New.Field8')\"\n                  ></v-select></v-col\n              ></v-row>\n              <v-row v-if=\"vshow >= 4\">\n                <v-col\n                  cols=\"5\"\n                  xs=\"5\"\n                  sm=\"5\"\n                  md=\"5\"\n                  style=\"padding-bottom: unset; padding-top: unset\"\n                >\n                  <v-text-field\n                    v-model=\"Perfil[3].Description\"\n                    :placeholder=\"$t('New.Field9')\"\n                    :rules=\"RulesG\"\n                    required\n                  ></v-text-field></v-col\n                ><v-col\n                  cols=\"3\"\n                  xs=\"3\"\n                  sm=\"3\"\n                  md=\"3\"\n                  style=\"padding-bottom: unset; padding-top: unset\"\n                >\n                  <v-text-field\n                    v-model=\"Perfil[3].Percentage\"\n                    :placeholder=\"$t('New.Field10')\"\n                    :rules=\"RulesG\"\n                    required\n                  ></v-text-field></v-col\n                ><v-col\n                  cols=\"4\"\n                  xs=\"4\"\n                  sm=\"4\"\n                  md=\"4\"\n                  style=\"padding-bottom: unset; padding-top: unset\"\n                >\n                  <v-select\n                    v-model=\"ptypeK4\"\n                    :items=\"TypeK\"\n                    item-value=\"value\"\n                    item-text=\"text\"\n                    :label=\"$t('New.Field8')\"\n                  ></v-select></v-col\n              ></v-row>\n              <v-row v-if=\"vshow >= 5\">\n                <v-col\n                  cols=\"5\"\n                  xs=\"5\"\n                  sm=\"5\"\n                  md=\"5\"\n                  style=\"padding-bottom: unset; padding-top: unset\"\n                >\n                  <v-text-field\n                    v-model=\"Perfil[4].Description\"\n                    :placeholder=\"$t('New.Field9')\"\n                    :rules=\"RulesG\"\n                    required\n                  ></v-text-field></v-col\n                ><v-col\n                  cols=\"3\"\n                  xs=\"3\"\n                  sm=\"3\"\n                  md=\"3\"\n                  style=\"padding-bottom: unset; padding-top: unset\"\n                >\n                  <v-text-field\n                    v-model=\"Perfil[4].Percentage\"\n                    :placeholder=\"$t('New.Field10')\"\n                    :rules=\"RulesG\"\n                    required\n                  ></v-text-field></v-col\n                ><v-col\n                  cols=\"4\"\n                  xs=\"4\"\n                  sm=\"4\"\n                  md=\"4\"\n                  style=\"padding-bottom: unset; padding-top: unset\"\n                >\n                  <v-select\n                    v-model=\"ptypeK5\"\n                    :items=\"TypeK\"\n                    item-value=\"value\"\n                    item-text=\"text\"\n                    :label=\"$t('New.Field8')\"\n                  ></v-select></v-col\n              ></v-row>\n\n              <v-btn\n                :disabled=\"!valid\"\n                color=\"success\"\n                class=\"mr-4\"\n                @click=\"Savevac\"\n              >\n                Guardar\n              </v-btn>\n              <v-btn color=\"error\" class=\"mr-4\" @click=\"Delete\">\n                Eliminar vacante\n              </v-btn>\n            </v-card-text>\n          </v-form>\n        </v-card>\n      </v-col>\n    </v-row>\n\n    <v-snackbar v-model=\"snackbar\" color=\"green\">\n      {{ snackbarText }}\n      <template v-slot:action=\"{ attrs }\">\n        <v-btn color=\"white\" text v-bind=\"attrs\" @click=\"snackbar = false\">\n          Cerrar\n        </v-btn>\n      </template>\n    </v-snackbar>\n  </v-container>\n</template>\n<script>\nimport axios from \"axios\";\nexport default {\n  name: \"Home\",\n  data() {\n    return {\n      filtro1: \"\",\n      filtro2: \"\",\n      valid: true,\n      idvac: 0,\n      page: this.$route.meta.breadcrum[0].name,\n      icon: this.$route.meta.breadcrum[0].icon,\n      Vacantes: [],\n      Perfil: [],\n      dialog: false,\n      dialogTerm: false,\n      vshow: 1,\n      Cnombre: \"\",\n      Capellidos: \"\",\n      Cestado: \"\",\n      Ccorreo: \"\",\n      Cdireccion: \"\",\n      Ctelefono: \"\",\n      Cimagen: \"\",\n      file: \"\",\n      dragging: false,\n      Terminosycondiciones: \"Terminos y condiciones\",\n      snackbar: \"\",\n      snackbarText: \"\",\n      pcompetencia1: \"\",\n      pponderacion1: \"\",\n      ptypeK1: \"1\",\n      pcompetencia2: \"\",\n      pponderacion2: \"\",\n      ptypeK2: \"1\",\n      pcompetencia3: \"\",\n      pponderacion3: \"\",\n      ptypeK3: \"1\",\n      pcompetencia4: \"\",\n      pponderacion4: \"\",\n      ptypeK4: \"1\",\n      pcompetencia5: \"\",\n      pponderacion5: \"\",\n      ptypeK5: \"1\",\n      englishlvlitems: [\n        { value: \"1\", text: \"No aplica\" },\n        { value: \"2\", text: \"Principiante\" },\n        { value: \"3\", text: \"Elemental\" },\n        { value: \"4\", text: \"Pre-intermedio\" },\n        { value: \"5\", text: \"Intermedio avanzado\" },\n        { value: \"6\", text: \"Avanzado\" },\n      ],\n      TypeK: [\n        { value: \"1\", text: \"Competencia Hard\" },\n        { value: \"2\", text: \"Competencia Soft\" },\n      ],\n      statesitems: [\n        { text: \"Aguascalientes\" },\n        { text: \"Baja California\" },\n        { text: \"Baja California Sur\" },\n        { text: \"Campeche\" },\n        { text: \"Chiapas\" },\n        { text: \"Chihuahua\" },\n        { text: \"Coahuila\" },\n        { text: \"Colima\" },\n        { text: \"Ciudad de México\" },\n        { text: \"Durango\" },\n        { text: \"Estado de México\" },\n        { text: \"Guanajuato\" },\n        { text: \"Guerrero\" },\n        { text: \"Hidalgo\" },\n        { text: \"Jalisco\" },\n        { text: \"Michoacán\" },\n        { text: \"Morelos\" },\n        { text: \"Nayarit\" },\n        { text: \"Nuevo León\" },\n        { text: \"Oaxaca\" },\n        { text: \"Puebla\" },\n        { text: \"Querétaro\" },\n        { text: \"Quintana Roo\" },\n        { text: \"San Luis Potosí\" },\n        { text: \"Sinaloa\" },\n        { text: \"Sonora\" },\n        { text: \"Tabasco\" },\n        { text: \"Tamaulipas\" },\n        { text: \"Tlaxcala\" },\n        { text: \"Veracruz\" },\n        { text: \"Yucatán\" },\n        { text: \"Zacatecas\" },\n      ],\n      RulesG: [(v) => !!v || \"Campo requerido\"],\n    };\n  },\n  mounted() {\n    this.idvac = this.$route.params.data;\n    this.Getpos(this.idvac);\n    this.Getprofile(this.idvac);\n  },\n  methods: {\n    Getpos(idv) {\n      let postdata = {\n        id: idv,\n      };\n      axios\n        .get(\"http://localhost/reclu/api/Showdetails.php\", {\n          params: postdata,\n        })\n        .then((response) => {\n          this.Vacantes = response.data[0];\n          if (this.Vacantes.ShowSalary == 1) {\n            this.Vacantes.ShowSalary = true;\n          } else {\n            this.Vacantes.ShowSalary = false;\n          }\n        })\n        .catch((error) => {\n          console.log(response.data);\n        });\n    },\n    Getprofile(idv) {\n      let postdata = {\n        id: idv,\n      };\n      axios\n        .get(\"http://localhost/reclu/api/apego.php\", {\n          params: postdata,\n        })\n        .then((response) => {\n          this.Perfil = JSON.parse(JSON.stringify(response.data));\n          this.vshow = this.Perfil.length;\n        })\n        .catch((error) => {\n          console.log(response.data);\n        });\n    },\n    Delete() {\n      let postdata = {\n        id: this.idvac,\n      };\n      axios\n        .get(\"http://localhost/reclu/api/Delete.php\", {\n          params: postdata,\n        })\n        .then((response) => {\n          alert(\"La vacante fue eliminada\");\n          this.$router.push(\"/\");\n        })\n        .catch((error) => {\n          console.log(error.data);\n        });\n    },\n    Savevac() {\n      let postdata = {\n        value1: this.Vacantes.VacancyName,\n        value2: this.Vacantes.Description,\n        value3: this.Vacantes.Location,\n        value4: this.Vacantes.MinSalary,\n        value5: this.Vacantes.MaxSalary,\n        value6: this.Vacantes.EnglishLevel,\n        value7: this.Vacantes.LaborExperience,\n        value8: this.Vacantes.Scholarship,\n        value9:\n          this.pcompetencia1 +\n          \"&&\" +\n          this.pponderacion1 +\n          \"&&\" +\n          this.ptypeK1 +\n          \"||\" +\n          this.pcompetencia2 +\n          \"&&\" +\n          this.pponderacion2 +\n          \"&&\" +\n          this.ptypeK2 +\n          \"||\" +\n          this.pcompetencia3 +\n          \"&&\" +\n          this.pponderacion3 +\n          \"&&\" +\n          this.ptypeK3 +\n          \"||\" +\n          this.pcompetencia4 +\n          \"&&\" +\n          this.pponderacion4 +\n          \"&&\" +\n          this.ptypeK4 +\n          \"||\" +\n          this.pcompetencia5 +\n          \"&&\" +\n          this.pponderacion5 +\n          \"&&\" +\n          this.ptypeK5,\n        value10: this.vshow,\n        value11: this.idvac,\n        value12: this.Vacantes.ShowSalary,\n      };\n      axios\n        .get(\"http://localhost/reclu/api/Update.php\", {\n          params: postdata,\n        })\n        .then((response) => {\n          this.snackbar = true;\n          this.snackbarText = \"Vacante modificada\";\n        })\n        .catch((error) => {\n          console.log(response.data);\n        });\n    },\n    Addmore(x) {\n      if (x == 1 && this.vshow > 1) {\n        this.vshow--;\n      }\n      if (x == 2 && this.vshow < 5) {\n        this.vshow++;\n      }\n    },\n    filter() {\n      this.$router.push({\n        name: \"Home\",\n        params: {\n          filter1: this.filtro1,\n          filter2: this.filtro2,\n        },\n      });\n    },\n  },\n};\n</script>\n"]}]}