m.title\" v-model=\"item.active\" :prepend-icon=\"item.action\" no-action>\r\n                    \r\n                    <template v-slot:activator>\r\n                        <v-list-item-content>\r\n                            <v-list-item-title v-text=\"item.title\"></v-list-item-title>\r\n                        </v-list-item-content>\r\n                    </template>\r\n\r\n                    <v-list nav dense>\r\n                        <v-list-item-group color=\"primary\" >\r\n                            <v-list-item v-for=\"child in item.items\" :key=\"child.title\">\r\n                                <router-link :to=\"child.url\">{{ child.title }}</router-link>\r\n                            </v-list-item>\r\n                        </v-list-item-group>\r\n                    </v-list>\r\n\r\n                </v-list-group>\r\n            </v-list>\r\n            <v-divider></v-divider>\r\n            <v-footer absolute dark>\r\n                <v-btn block class=\"primary\" @click=\"Logout()\">\r\n                    Cerrar session\r\n                </v-btn>\r\n            </v-footer>\r\n        </v-navigation-drawer>\r\n    </v-sheet>\r\n</template>\r\n\r\n<script>\r\n    import { mapState } from 'vuex'\r\n    import router from '../router'\r\n\r\n    export default {\r\n        name: 'Sidebar',\r\n        data () {\r\n            const langu = localStorage.getItem('lang') || 'es';\r\n            return {\r\n                drawer: null,\r\n                Username:\"Rogelio\",\r\n                languages: [\r\n                    { shortCode: 'en', text: 'English' },\r\n                    { shortCode: 'es', text: 'Español' },\r\n                ],\r\n                setLocale: langu,\r\n                items: [\r\n                    {\r\n                        action: 'mdi-account',\r\n                        items: [\r\n                            { title: 'Mi perfil', url:'/Perfil' },\r\n                            { title: 'Mi calendario', url:'/calendar' }\r\n                        ],\r\n                        title: 'Mi persona',\r\n                    },\r\n                    {\r\n                        action: 'mdi-school',\r\n                        items: [\r\n                            { title: 'Univeraidad urrea' },\r\n                        ],\r\n                         title: 'Mi crecimiento',\r\n                    },\r\n                    {\r\n                        action: 'mdi-city',\r\n                        items: [\r\n                            { title: 'Registrar visita',url:'/registro_visitantes' },\r\n                            { title: 'Evaluacion 360 para plantas',url:'/360_plantas' },\r\n                        ],\r\n                         title: 'Mi Empresa',\r\n                    },\r\n                    {\r\n                        action: 'mdi-account-multiple',\r\n                        items: [\r\n                            { title: 'Administracion de puestos',url:'/administracion_puestos' },\r\n                        ],\r\n                         title: 'Mi Equipo',\r\n                    },\r\n                    {\r\n                        action: 'mdi-view-dashboard',\r\n                        items: [\r\n                            { title: 'Evaluacion 360 para plabtas',url:'/360_plantas_adm' },\r\n                            { title: 'Crear oferta economica',url:'/ofertas_economicas' },\r\n                            { title: 'Ofertas economica pendientes',url:'/ofertas_economicas_adm' },\r\n                        ],\r\n                         title: 'Administracion',\r\n                    }\r\n                ]\r\n            }\r\n        },\r\n        methods:{\r\n            Logout(){\r\n                localStorage.clear(); \r\n                window.location.reload()\r\n            },\r\n            handleChange(event){\r\n                localStorage.setItem('lang',event)\r\n                window.location.reload()\r\n            }\r\n        },\r\n        computed:{\r\n            ...mapState(['usuario'])\r\n        }\r\n    }\r\n</script>\r\n<style>\r\n    .v-avatar{\r\n        height: 75px !important;\r\n        min-width: 75px !important;\r\n        width: 75px !important;\r\n    }\r\n</style>"]}]}