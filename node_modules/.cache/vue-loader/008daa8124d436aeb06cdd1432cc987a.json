rios\"\n                        item-value=\"value\"\n                        item-text=\"text\"\n                        :label=\"$t('Pefil-usuario.Turno')\"\n                      ></v-select>\n                    </v-col>\n                  </v-row>\n\n                  <v-row>\n                    <v-col md=\"3\">\n                      <v-btn\n                        small\n                        color=\"success\"\n                        style=\"width: 100%\"\n                        @click=\"ChangeT()\"\n                      >\n                        {{ $t(\"UserInfo.Savec\") }}\n                      </v-btn>\n                    </v-col>\n                  </v-row>\n                </v-tab-item>\n              </v-tabs-items>\n            </v-container>\n          </v-card-text>\n        </v-card>\n      </v-col>\n    </v-row>\n    <v-dialog\n      v-model=\"dialogFired\"\n      fullscreen\n      hide-overlay\n      transition=\"dialog-bottom-transition\"\n      scrollable\n    >\n      <v-card tile>\n        <v-toolbar flat dark color=\"primary\">\n          <v-btn icon dark @click=\"dialogFired = false\">\n            <v-icon>mdi-close</v-icon>\n          </v-btn>\n          <v-toolbar-title>{{ $t(\"UserInfo.Field2\") }}</v-toolbar-title>\n        </v-toolbar>\n        <v-card-text>\n          <v-form ref=\"form\" v-model=\"valid\" lazy-validation>\n            <v-menu\n              ref=\"menu1\"\n              v-model=\"menu1\"\n              :close-on-content-click=\"false\"\n              :return-value.sync=\"date1\"\n              transition=\"scale-transition\"\n              offset-y\n              min-width=\"auto\"\n            >\n              <template v-slot:activator=\"{ on, attrs }\">\n                <v-combobox\n                  v-model=\"date1\"\n                  :label=\"$t('UserInfo.Field3')\"\n                  readonly\n                  v-bind=\"attrs\"\n                  v-on=\"on\"\n                ></v-combobox>\n            