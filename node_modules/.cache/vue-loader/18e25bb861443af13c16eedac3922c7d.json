=\"center\">\n                <v-col cols=\"12\" align=\"center\" justify=\"center\">\n                  <v-card elevation=\"24\">\n                    <form\n                      id=\"Sessionform\"\n                      @submit=\"Iniciarsesion\"\n                      v-on:keyup.enter=\"Iniciarsesion\"\n                    >\n                      <div style=\"padding:5px\">\n                        <v-img\n                          src=\"../assets/logo.png\"\n                          max-height=\"150\"\n                          max-width=\"250\"\n                        ></v-img>\n                        <h3>Inicia session para ingresar al portal</h3>\n                      </div>\n                      <v-card-text>\n                        <v-text-field\n                          v-model=\"usuario.username\"\n                          :rules=\"Rules\"\n                          label=\"Nombre de usuario\"\n                          required\n                        >\n                        </v-text-field>\n                        <v-text-field\n                          v-model=\"usuario.password\"\n                          :rules=\"Rules\"\n                          label=\"Contraseña\"\n                          :append-icon=\"show ? 'mdi-eye' : 'mdi-eye-off'\"\n                          :type=\"show ? 'text' : 'password'\"\n                          @click:append=\"show = !show\"\n                          required\n                        >\n                        </v-text-field>\n                        <v-btn\n                          color=\"primary\"\n                          :loading=\"loadBtn\"\n                          :disabled=\"loadBtn\"\n                          @click=\"Iniciarsesion\"\n                        >\n                          Ingresar\n                        </v-btn>\n                      </v-card-text>\n                    </form>\n                  </v-card>\n                </v-col>\n              </v-row>\n\n              <v-row>\n                <v-col cols=\"6\" align=\"center\" justify=\"center\">\n                  <v-card color=\"#0072b3\" dark height=\"200\" elevation=\"24\">\n                    <div style=\"padding:5px;margin:11px\" height=\"50\">\n                      <h3>Rutas de trasporte</h3>\n                    </div>\n                    <v-card-text>\n                      <v-icon x-large>mdi-bus</v-icon>\n                    </v-card-text>\n                    <v-btn\n                      outlined\n                      rounded\n                      small\n                      @click.stop=\"dialogTrasporte = true\"\n                    >\n                      Ver rutas\n                    </v-btn>\n                  </v-card>\n                </v-col>\n                <v-col cols=\"6\" align=\"center\" justify=\"center\">\n                  <v-card color=\"#0072b3\" dark height=\"200\" elevation=\"24\">\n                    <div style=\"padding:5px;margin:11px\" height=\"50\">\n                      <h3>Ponte en contactanos</h3>\n                    </div>\n                    <v-card-text>\n                      <v-icon x-large>mdi-email</v-icon>\n                    </v-card-text>\n                    <v-btn\n                      outlined\n                      rounded\n                      small\n                      @click.stop=\"dialogContactanos = true\"\n                    >\n                      Contacto\n                    </v-btn>\n                  </v-card>\n                </v-col>\n              </v-row>\n            </v-col>\n          </v-row>\n        </v-col>\n      </v-row>\n      <!--Modal para rutas de trasporte-->\n      <v-dialog v-model=\"dialogTrasporte\" scrollable max-width=\"400\">\n        <v-card>\n          <v-card-title class=\"headline\">\n            Rutas de trasporte\n          </v-card-title>\n          <v-divider></v-divider>\n          <v-card-text style=\"height: 300px;\">\n            <v-list nav dense>\n              <v-list-item-group color=\"primary\">\n                <v-list-item\n                  v-for=\"(Ruta, i) in Rutas\"\n                  :key=\"i\"\n                  :href=\"Ruta.Url\"\n                  target=\"_blank\"\n                >\n                  <v-list-item-icon>\n                    <v-icon>mdi-tag</v-icon>\n                  </v-list-item-icon>\n                  <v-list-item-content>\n                    <v-list-item-title v-text=\"Ruta.Nombre\"></v-list-item-title>\n                  </v-list-item-content>\n                </v-list-item>\n              </v-list-item-group>\n            </v-list>\n          </v-card-text>\n          <v-divider></v-divider>\n          <v-card-actions>\n            <v-btn color=\"blue darken-1\" text @click=\"dialogTrasporte = false\">\n              Cerrar\n            </v-btn>\n          </v-card-actions>\n        </v-card>\n      </v-dialog>\n      <!--Modal para contactanos-->\n      <v-dialog v-model=\"dialogContactanos\" scrollable>\n        <v-card>\n          <v-card-title class=\"headline\">\n            Contactanos\n          </v-card-title>\n          <v-divider></v-divider>\n          <v-card-text>\n            <v-row align=\"center\" justify=\"center\">\n              <v-col cols=\"12\" xs=\"12\" sm=\"6\" md=\"6\">\n                <p>\n                  En Grupo Urrea estamos comprometidos con<br />\n                  <b style=\"color:blue\">La política de puertas abiertas</b\n                  ><br />\n                  Si sufres alguna situación de:<br />\n                  - Inconformidad<br />\n                  - Abuso<br />\n                  - Acoso sexual u otros conflictos<br /><br />\n\n                  Envíanos tu inquietud y se manejara de forma 100%\n                  confidencial.<br /><br />\n\n                  También puedes hacerlo a través de la Línea confidencial\n                  URREA:01 800 22 87732\n                </p>\n              </v-col>\n              <v-col cols=\"12\" xs=\"12\" sm=\"6\" md=\"6\">\n                <v-text-field outlined label=\"Asunto\"></v-text-field>\n                <v-text-field\n                  outlined\n                  label=\"Nombre y/o Departamento\"\n                ></v-text-field>\n                <v-textarea outlined rows=\"2\">\n                  <template v-slot:label>\n                    <div>\n                      Comentarios\n                    </div>\n                  </template>\n                </v-textarea>\n                <v-btn block color=\"success\" elevation=\"15\">Enviar</v-btn>\n              </v-col>\n            </v-row>\n          </v-card-text>\n          <v-divider></v-divider>\n          <v-card-actions>\n            <v-btn\n              color=\"blue darken-1\"\n              text\n              @click=\"dialogContactanos = false\"\n            >\n              Cerrar\n            </v-btn>\n          </v-card-actions>\n        </v-card>\n      </v-dialog>\n      <!--<v-snackbar v-model=\"snackbar\" color=\"red\">\n                {{ errorText }}\n                <template v-slot:action=\"{ attrs }\">\n                    <v-btn\n                    v-bind=\"attrs\"\n                    @click=\"snackbar = false\"\n                    >\n                    Cerrar\n                    </v-btn>\n                </template>\n            </v-snackbar>-->\n    </v-container>\n  </v-app>\n</template>\n<script>\nimport { mapActions, mapState } from \"vuex\";\nimport axios from \"axios\";\nexport default {\n  name: \"Login\",\n  data() {\n    return {\n      dialogTrasporte: false,\n      dialogContactanos: false,\n      loadBtn: false,\n      loader: null,\n      Rutas: null,\n      Slides: [\n        {\n          src: \"http://desarrollohumano.urrea.com/IMAGENES/noticias/1.jpg\",\n        },\n      ],\n      usuario: {\n        username: \"\",\n        password: \"\",\n      },\n      show: false,\n      Rules: [(v) => !!v || \"Campo requerido\"],\n      sessionWatcher: null,\n    };\n  },\n  methods: {\n    ...mapActions([\"authUser\"]),\n    Iniciarsesion() {\n      this.loadBtn = true;\n      this.authUser(this.usuario);\n    },\n  },\n  created() {\n    axios\n      .get(\"http://192.168.1.238:92/api/rutasTrasporte/get.php\")\n      .then((result) => {\n        this.Rutas = result.data;\n      });\n  },\n};\n</script>\n<style>\n.content {\n  background: rgb(0, 114, 179);\n  background: linear-gradient(\n    90deg,\n    rgba(0, 114, 179, 1) 60%,\n    rgb(255, 255, 255) 60%\n  );\n}\n@media (max-width: 600px) {\n  .content {\n    background: rgb(0, 114, 179);\n    background: linear-gradient(\n      180deg,\n      rgba(0, 114, 179, 1) 30%,\n      rgb(255, 255, 255) 30%\n    );\n  }\n}\n</style>\n"]}]}