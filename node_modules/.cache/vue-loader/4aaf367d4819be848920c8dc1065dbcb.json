ages\" color=\"red\" overlap>\r\n          <v-icon> mdi-bell </v-icon>\r\n        </v-badge>\r\n      </v-btn>\r\n      <v-btn icon @click.stop=\"DrawerSett = !DrawerSett\">\r\n        <v-icon> mdi-image-filter-vintage </v-icon>\r\n      </v-btn>\r\n      <v-menu offset-y>\r\n        <template v-slot:activator=\"{ on, attrs }\">\r\n          <v-btn icon v-bind=\"attrs\" v-on=\"on\" style=\"color: black\">\r\n            <v-icon>mdi-dots-vertical</v-icon>\r\n          </v-btn>\r\n        </template>\r\n        <v-list nav dense>\r\n          <v-list-item-group>\r\n            <v-list-item>\r\n              <v-list-item-title @click=\"Logout()\">{{\r\n                $t(\"Sesion\")\r\n              }}</v-list-item-title>\r\n            </v-list-item>\r\n          </v-list-item-group>\r\n        </v-list>\r\n      </v-menu>\r\n    </v-toolbar>\r\n    <!--Menu-->\r\n    <v-navigation-drawer v-model=\"Drawer\" dark permanent fixed style = \"background:#0070BA\">\r\n      <v-list>\r\n        <v-list-item class=\"px-2\">\r\n          <v-list-item-avatar>\r\n            <v-img\r\n              src=\"https://randomuser.me/api/portraits/women/85.jpg\"\r\n            ></v-img>\r\n          </v-list-item-avatar>\r\n        </v-list-item>\r\n      </v-list>\r\n      <v-divider></v-divider>\r\n      <v-list nav dense>\r\n        <v-list-group\r\n          color=\"primary\"\r\n          v-for=\"item in items\"\r\n          :key=\"item.title\"\r\n          v-model=\"item.active\"\r\n          :prepend-icon=\"item.action\"\r\n          no-action\r\n        >\r\n          <template v-slot:activator>\r\n            <v-list-item-content>\r\n              <v-list-item-title v-text=\"item.title\"></v-list-item-title>\r\n            </v-list-item-content>\r\n          </template>\r\n          <v-list nav dense>\r\n            <v-list-item-group>\r\n              <v-list-item v-for=\"child in item.items\" :key=\"child.title\">\r\n                <router-link :to=\"child.url\">{{ child.title }}</router-link>\r\n              </v-list-item>\r\n            </v-list-item-group>\r\n          </v-list>\r\n        </v-list-group>\r\n      </v-list>\r\n    </v-navigation-drawer>\r\n    <!--Settings-->\r\n    <v-navigation-drawer v-model=\"DrawerSett\" absolute temporary>\r\n      <v-list-item>\r\n        <h3>{{ $t(\"Configuracion\") }}</h3>\r\n      </v-list-item>\r\n      <v-divider></v-divider>\r\n      <v-card-text>\r\n        <v-row no-gutters class=\"align-center\">\r\n          <v-col cols=\"10\" class=\"text-primary\">\r\n            Mantener sesion iniciada\r\n          </v-col>\r\n          <v-col cols=\"2\">\r\n            <v-switch v-model=\"swSession\"></v-switch>\r\n          </v-col>\r\n        </v-row>\r\n        <v-row>\r\n          <v-col cols=\"12\">\r\n            <v-select\r\n              v-model=\"SetLocale\"\r\n              :label=\"$t('Idioma')\"\r\n              :items=\"Languages\"\r\n              item-value=\"shortCode\"\r\n              item-text=\"text\"\r\n              @change=\"handleChange\"\r\n              hide-details\r\n              id=\"langSelect\"\r\n            ></v-select>\r\n          </v-col>\r\n        </v-row>\r\n      </v-card-text>\r\n    </v-navigation-drawer>\r\n  </v-sheet>\r\n</template>\r\n\r\n<script>\r\nimport { mapState } from \"vuex\";\r\nimport router from \"../router\";\r\nexport default {\r\n  Name: \"Sidebar\",\r\n  data() {\r\n    const LANGU = localStorage.getItem(\"lang\") || \"es\";\r\n    const Storeduser = JSON.parse(localStorage.getItem(\"USER_AUTH\"));\r\n    return {\r\n      Drawer: null,\r\n      DrawerSett: null,\r\n      Messages: 1,\r\n      show: false,\r\n      SetLocale: LANGU,\r\n      User: Storeduser,\r\n      swSession: true,\r\n      Languages: [\r\n        { shortCode: \"en\", text: \"English\" },\r\n        { shortCode: \"es\", text: \"Espa√±ol\" },\r\n      ],\r\n      items: [\r\n        {\r\n          action: \"mdi-account\",\r\n          items: [\r\n            { title: \"Mi perfil\", url: \"/Perfil\" },\r\n            { title: \"Mi calendario\", url: \"/calendar\" },\r\n          ],\r\n          title: \"Mi persona\",\r\n        },\r\n        {\r\n          action: \"mdi-city\",\r\n          items: [\r\n            { title: \"Registrar visita\", url: \"/registro_visitantes\" },\r\n            { title: \"Evaluacion 360 para plantas\", url: \"/360_plantas\" },\r\n          ],\r\n          title: \"Mi Empresa\",\r\n        },\r\n        {\r\n          action: \"mdi-account-multiple\",\r\n          items: [\r\n            {\r\n              title: \"Administracion de puestos\",\r\n              url: \"/administracion_puestos\",\r\n            },\r\n          ],\r\n          title: \"Mi Equipo\",\r\n        },\r\n        {\r\n          action: \"mdi-view-dashboard\",\r\n          items: [\r\n            { title: \"Evaluacion 360 para plabtas\", url: \"/360_plantas_adm\" },\r\n            { title: \"Crear oferta economica\", url: \"/ofertas_economicas\" },\r\n            {\r\n              title: \"Ofertas economica pendientes\",\r\n              url: \"/ofertas_economicas_adm\",\r\n            },\r\n          ],\r\n          title: \"Administracion\",\r\n        },\r\n      ],\r\n    };\r\n  },\r\n  methods: {\r\n    Logout() {\r\n      localStorage.clear();\r\n      window.location.reload();\r\n    },\r\n    handleChange(event) {\r\n      localStorage.setItem(\"lang\", event);\r\n      window.location.reload();\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style>\r\n    a{\r\n        color: #FFFFFF !important;\r\n        caret-color: #FFFFFF !important;\r\n        text-decoration: none;\r\n        font-size:14px\r\n    }\r\n    .v-list-item__title{\r\n        color:#FFFFFF !important;\r\n    }\r\n.v-list__group__header__prepend-icon >>> .v-icon {\r\n    color: red;\r\n}\r\n</style>"]}]}